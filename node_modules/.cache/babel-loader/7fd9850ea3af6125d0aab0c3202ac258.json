{"ast":null,"code":"import \"jquery/dist/jquery.min.js\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"datatables.net-dt/js/dataTables.dataTables\";\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\nimport $ from \"jquery\"; // DataTable.use(DataTablesCore);\n\nexport default {\n  components: {// DataTable,\n  },\n\n  data() {\n    const storageData = JSON.parse(localStorage.getItem('user'));\n    return {\n      backend_url: process.env.VUE_APP_API_URL,\n      storageData,\n      user: null,\n      departments: null,\n      designations: null,\n      transferType: null,\n      office: null,\n      edit: false,\n      url: this.backendUrl + \"/api/users\",\n      basicInfo: {\n        name: '',\n        mobileNumber: '',\n        email: '',\n        designation: '',\n        designation_id: '',\n        image: ''\n      },\n      trainingRecordById: [],\n      trainingList1: [],\n      //         <!-- \"employee_id\": 1,\n      //   \"employee_name\": \"Shakiur\",\n      //   \"id\": 2,\n      //   \"training_center_name\": \"322\",\n      //   \"training_name\": \"322\",\n      //   \"training_score\": 322,\n      //   \"training_feedback\": \"322\",\n      //   \"training_strt_date\": \"2023-10-29\",\n      //   \"training_end_date\": \"2023-10-29\",\n      //   \"description\": \"322\",\n      //   \"status\": 0,\n      //   \"created_at\": \"2023-10-29T06:30:12.000000Z\",\n      //   \"updated_at\": \"2023-10-29T06:42:12.000000Z\" -->\n      training: {\n        employee_id: null,\n        training_center_name: null,\n        training_name: null,\n        training_score: null,\n        training_feedback: null,\n        training_strt_date: null,\n        training_end_date: null\n      }\n    };\n  },\n\n  computed: {},\n  methods: {\n    editbutton(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificUserTraining/' + id).then(response => {\n        this.trainingRecordById = response.data.data;\n        console.log(this.trainingRecordById);\n        this.training.id = id;\n        this.training.employee_id = this.trainingRecordById.employee_id;\n        this.training.training_center_name = this.trainingRecordById.training_center_name;\n        this.training.training_name = this.trainingRecordById.training_name;\n        this.training.training_score = this.trainingRecordById.training_score;\n        this.training.training_feedback = this.trainingRecordById.training_feedback;\n        this.training.training_strt_date = this.trainingRecordById.training_strt_date;\n        this.training.training_end_date = this.trainingRecordById.training_end_date;\n        this.training.description = this.trainingRecordById.description;\n        this.edit = true;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    deletebutton(id) {\n      this.axios.delete(this.backend_url + 'deleteTrainingRecord/' + id).then(res => {\n        console.log('Delete Successfully', res);\n        this.getTrainingList();\n        $('#datatable').DataTable().destroy();\n      });\n    },\n\n    viewDetails(id) {\n      console.log(id); // <!-- training: {\n      //     employee_id: null,\n      //     training_center_name: null,\n      //     training_name: null,\n      //     training_score: null,\n      //     training_feedback: null,\n      //     training_strt_date: null,\n      //     training_end_date: null,\n      // } -->\n\n      this.axios.get(this.backend_url + 'specificUserTransferRecord/' + id).then(response => {\n        this.trainingRecordById = response.data.data;\n        console.log(this.trainingRecordById);\n        this.training.id = id;\n        this.training.employee_id = this.trainingRecordById.employee_id;\n        this.training.training_center_name = this.trainingRecordById.training_center_name;\n        this.training.training_name = this.trainingRecordById.training_name;\n        this.training.training_score = this.trainingRecordById.training_score;\n        this.training.training_feedback = this.trainingRecordById.training_feedback;\n        this.training.training_strt_date = this.trainingRecordById.training_strt_date;\n        this.training.training_end_date = this.trainingRecordById.training_end_date;\n        this.training.description = this.trainingRecordById.description;\n        this.edit = false;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    getTrainingList() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n\n        this.training.employee_id = this.user; // console.log(this.storageData.id);\n\n        this.axios.get(this.backend_url + 'specificUserTrainingRecordByEmployeeId/' + this.user).then(response => {\n          this.trainingList1 = response.data; // Initialize DataTable here\n\n          setTimeout(function () {\n            $(\"#datatable\").DataTable({\n              \"processing\": true\n            });\n          }, 100);\n        }).catch(e => {\n          console.log(e);\n        });\n      });\n    },\n\n    getDepartmentList() {\n      this.axios.get(this.backend_url + 'getDepartment').then(res => {\n        // console.log(res.data.data);\n        this.departments = res.data.data;\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data; // console.log(this.designations);\n      });\n    },\n\n    getTransfertype() {\n      this.axios.get(this.backend_url + 'transferType').then(res => {\n        this.transferType = res.data.list; // console.log('fddsfsdf');\n        // console.log(this.transferType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n\n        this.training.employee_id = this.user; // console.log(this.storageData.id);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list; // console.log(this.office);\n      });\n    },\n\n    saveTraining() {\n      // this.training.employee_id = this.storageData.id;\n      this.edit = false;\n      this.axios.post(this.backend_url + 'addTrainingRecord', this.training).then(res => {\n        console.log(res);\n        $(\"#exampleModal\").modal('hide');\n      });\n      this.getTrainingList();\n      $('#datatable').DataTable().destroy();\n    },\n\n    EditTraining() {\n      this.axios.post(this.backend_url + `updateTrainingRecord/${this.training.id}`, this.training).then(res => {\n        console.log(res);\n        $(\"#exampleModal\").modal('hide');\n      });\n      this.getTrainingList();\n      $('#datatable').DataTable().destroy();\n    }\n\n  },\n\n  created() {\n    this.getTrainingList();\n    this.getDepartmentList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser();\n    this.getOffice();\n  },\n\n  mounted() {},\n\n  watch: {}\n};","map":{"version":3,"mappings":"AAoPA,OAAO,2BAAP;AACA,OAAO,sCAAP;AACA,OAAO,4CAAP;AACA,OAAO,iDAAP;AACA,OAAOA,CAAP,MAAc,QAAd,C,CACA;;AACA,eAAe;EACXC,UAAU,EAAE,CACR;EADQ,CADD;;EAOXC,IAAI,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX,CAApB;IAEA,OAAO;MACHC,WAAW,EAAEC,OAAO,CAACC,GAAR,CAAYC,eADtB;MAEHR,WAFG;MAGHS,IAAI,EAAE,IAHH;MAIHC,WAAW,EAAE,IAJV;MAKHC,YAAY,EAAE,IALX;MAMHC,YAAY,EAAE,IANX;MAOHC,MAAM,EAAE,IAPL;MAQHC,IAAI,EAAE,KARH;MASHC,GAAG,EAAE,KAAKC,UAAL,GAAkB,YATpB;MAUHC,SAAS,EAAE;QACPC,IAAI,EAAE,EADC;QAEPC,YAAY,EAAE,EAFP;QAGPC,KAAK,EAAE,EAHA;QAIPC,WAAW,EAAE,EAJN;QAKPC,cAAc,EAAE,EALT;QAMPC,KAAK,EAAE;MANA,CAVR;MAkBHC,kBAAkB,EAAE,EAlBjB;MAmBHC,aAAa,EAAE,EAnBZ;MAoBH;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACAC,QAAQ,EAAE;QACNC,WAAW,EAAE,IADP;QAENC,oBAAoB,EAAE,IAFhB;QAGNC,a