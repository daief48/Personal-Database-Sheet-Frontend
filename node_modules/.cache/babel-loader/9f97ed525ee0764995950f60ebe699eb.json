URL","user","departments","designations","transferType","office","edit","url","backendUrl","basicInfo","name","mobileNumber","email","designation","designation_id","image","transferRecordById","promotionList1","promotion","employee_id","transfer_type","transfer_order","transfer_order_number","to_office","from_office","to_department","from_department","to_designation","from_designation","transfer_date","join_date","transfer_letter","computed","methods","editbutton","id","console","log","axios","get","then","response","employee_name","catch","e","deletebutton","delete","res","getPromotionList","DataTable","destroy","$toast","error","viewDetails","userDetail","setTimeout","getDepartmentList","getDesignationList","getTransfertype","list","getUser","getOffice","savePromotion","post","modal","success","EditTranfer","created","mounted","watch"],"sourceRoot":"","sources":["C:\\Users\\Jubair\\Desktop\\pds\\src\\components\\Promotion\\Promotion.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h5><i class=\"fa fa-thin fa-arrow-left\"></i> Transfer</h5>\r\n        <div class=\"card card-default\">\r\n            <div class=\"card-body\">\r\n                <div style=\"display: flex;justify-content: space-between;\">\r\n                    <h3>My Transfer History</h3>\r\n                    <a href=\"/\" class=\"btn btn-outline-primary mb-2\" data-toggle=\"modal\" data-target=\"#exampleModal\">+ Add\r\n                        Transfer</a>\r\n\r\n                    <!-- hidden model  -->\r\n                    <!-- Add and Edit Modal -->\r\n\r\n                    <div class=\"modal fade\" id=\"exampleModal\" tabindex=\"-1\" role=\"dialog\"\r\n                        aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n                        <div class=\"modal-dialog\" role=\"document\">\r\n                            <div class=\"modal-content\" style=\"width: 82vw;left: -457px;top: 164px;\">\r\n                                <div class=\"modal-header\">\r\n                                    <h5 class=\"modal-title\" id=\"exampleModalLabel\">Promotion</h5>\r\n                                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                                        <span aria-hidden=\"true\">&times;</span>\r\n                                    </button>\r\n                                </div>\r\n                                <div class=\"modal-body\">\r\n                                    <div class=\"row\">\r\n                                        <div class=\"col-4\">\r\n                                            <div class=\"form-group\">\r\n                                                <label for=\"exampleInputEmail1\">Employee ID</label>\r\n                                                <input type=\"number\" class=\"form-control\" id=\"exampleInputEmail1\"\r\n                                                    aria-describedby=\"emailHelp\" placeholder=\"Enter Employee ID\">\r\n\r\n                                            </div>\r\n\r\n\r\n                                            <div class=\"form-group mr-2 w-50\">\r\n                                                <label for=\"exampleFormControlSelect1\">To Designation</label>\r\n                                                <select class=\"form-control\" id=\"exampleFormControlSelect1\"\r\n                                                    >\r\n\r\n                                                    <option v-for=\"designation in designations\" :key=\"designation.id\"\r\n                                                        v-bind:value=\"designation.id\">\r\n                                                        {{ designation.designation_name }}</option>\r\n\r\n                                                </select>\r\n\r\n                                            </div>\r\n\r\n                                        </div>\r\n\r\n                                        <div class=\"col-4\">\r\n                                            <div class=\"form-group\">\r\n                                                <label for=\"exampleInputEmail1\">Promotion Reference Number</label>\r\n                                                <input type=\"number\" class=\"form-control\" id=\"exampleInputEmail1\"\r\n                                                    aria-describedby=\"emailHelp\" placeholder=\"Reference number here\">\r\n\r\n                                            </div>\r\n\r\n\r\n                                            <div class=\"form-group\">\r\n                                                <label for=\"exampleInputEmail1\">Promotion Date</label>\r\n                                                <input type=\"date\" class=\"form-control\" id=\"exampleInputEmail1\"\r\n                                                    aria-describedby=\"emailHelp\" placeholder=\"Enter email\">\r\n\r\n                                            </div>\r\n                                        </div>\r\n\r\n\r\n\r\n                                        <div class=\"col-4\">\r\n                                            <div class=\"form-group\">\r\n                                                <label for=\"exampleInputEmail1\">Description</label>\r\n                                                <textarea class=\"form-control \" id=\"exampleFormControlTextarea1\" rows=\"5\"\r\n                                                    placeholder=\"Write your comment here\"></textarea>\r\n\r\n                                            </div>\r\n\r\n\r\n\r\n\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"modal-footer d-flex justify-content-center\">\r\n                                    <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Cancel</button>\r\n                                    <button type=\"button\" class=\"btn btn-primary\">Save</button>\r\n                                </div>\r\n                            </div>\r\n\r\n\r\n\r\n\r\n\r\n                        </div>\r\n                    </div>\r\n                    <!--End Add and Edit Modal -->\r\n\r\n\r\n                    <!-- Transter Details Modal -->\r\n\r\n                    <div class=\"modal fade\" id=\"exampleModal2\" tabindex=\"-1\" role=\"dialog\"\r\n                        aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n                        <div class=\"modal-dialog\" role=\"document\">\r\n                            <div class=\"modal-content\" style=\"width: 121%; top: 164px;\">\r\n                                <div class=\"modal-header\">\r\n                                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                                        <span aria-hidden=\"true\">&times;</span>\r\n                                    </button>\r\n                                </div>\r\n                                <div class=\"modal-body card\">\r\n                                    <div class=\"card-body d-flex flex-column\">\r\n                                        <h5 class=\"modal-title text-center\" id=\"exampleModalLabel\" style=\"font-size: 25px;\">\r\n                                            Transfer Details\r\n                                        </h5>\r\n                                        <hr>\r\n                                        <h1 class=\"card-title \">Transfer Type: {{ promotion.transfer_type }}</h1>\r\n                                        <h5 class=\"card-title\">Transfer Order: {{ promotion.transfer_order }}</h5>\r\n                                        <h5 class=\"card-title\">Transfer Order Number: {{ promotion.transfer_order_number }}\r\n                                        </h5>\r\n                                        <h5 class=\"card-title\">To Office: {{ promotion.to_office }}</h5>\r\n                                        <h5 class=\"card-title\">From Office: {{ promotion.from_office }}</h5>\r\n                                        <h5 class=\"card-title\">To Designation: {{ promotion.to_designation }}</h5>\r\n                                        <h5 class=\"card-title\">From Designation: {{ promotion.from_designation }}</h5>\r\n                                        <h5 class=\"card-title\">Transfer Date: {{ promotion.transfer_date }}</h5>\r\n                                        <h5 class=\"card-title\">Join Date: {{ promotion.join_date }}</h5>\r\n                                        <h5 class=\"card-title\">Transfer Letter: {{ promotion.transfer_letter }}</h5>\r\n                                        <h5 class=\"card-title\">To Department: {{ promotion.to_department }}</h5>\r\n                                        <h5 class=\"card-title\">From Department: {{ promotion.from_department }}</h5>\r\n                                    </div>\r\n                                </div>\r\n\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n                    <!-- End Transter Details Modal -->\r\n\r\n\r\n                </div>\r\n\r\n                <hr>\r\n\r\n                <div class=\"row\">\r\n                    <div class=\"col-md-12\">\r\n\r\n                        <table class=\"table text-center\" id=\"datatable\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>ID</th>\r\n                                    <th>Employee</th>\r\n                                    <th>Promotion Ref Number</th>\r\n                                    <th>Promoted Designation</th>\r\n                                    <th>Promotion Date</th>\r\n                                    <th>Description</th>\r\n                                    <th>Status</th>\r\n                                    <th>Action</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                <!-- \"employee_id\": 1,\r\n      \"employee_name\": \"Shakiur\",\r\n      \"id\": 3,\r\n      \"name\": null,\r\n      \"promotion_ref_number\": \"sdf\",\r\n      \"promoted_designation\": 22,\r\n      \"promotion_date\": \"2023-03-28\",\r\n      \"description\": \"sfd\",\r\n      \"status\": 1,\r\n      \"created_at\": \"2023-10-30T08:19:36.000000Z\",\r\n      \"updated_at\": \"2023-10-30T08:23:44.000000Z\" -->\r\n                                <tr v-for=\"(item, index) in promotionList1.data\" :key=\"index\">\r\n                                    <td>{{ item.id }}</td>\r\n                                    <td>{{ item.employee_name }}</td>\r\n                                    <td>{{ item.promotion_ref_number }}</td>\r\n                                    <td>{{ item.designation_name }}</td>\r\n                                    <td>{{ item.promotion_date }}</td>\r\n                                    <td>{{ item.description }}</td>\r\n                                    <td>\r\n                                        <p v-if=\"item.status == 1\" class=\"text-warning font-weight-bold\"> Verified</p>\r\n                                        <p v-else class=\"text-success font-weight-bold\">Pending</p>\r\n                                    </td>\r\n                                    <td style=\"width: 185px;\">\r\n                                        <button v-if=\"item.status === 1\" @click=\"viewDetails(item.id)\"\r\n                                            class=\"btn btn-outline-primary mr-2\"\r\n                                            style=\"border: 3px solid;font-weight: bolder; width: 124px;\" data-toggle=\"modal\"\r\n                                            data-target=\"#exampleModal2\">\r\n                                            View Details\r\n                                        </button>\r\n                                        <div v-else>\r\n                                            <button @click=\"editbutton(item.id)\" class=\"btn btn-primary mr-2\"\r\n                                                data-toggle=\"modal\" data-target=\"#exampleModal\">\r\n                                                Edit\r\n                                            </button>\r\n                                            <button @click=\"deletebutton(item.id)\" class=\"btn btn-danger mr-2\">\r\n                                                Delete\r\n                                            </button>\r\n                                        </div>\r\n\r\n\r\n                                    </td>\r\n\r\n                                </tr>\r\n\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- Modal Start -->\r\n\r\n\r\n\r\n    </div>\r\n</template>\r\n    \r\n<script>\r\n\r\nimport \"jquery/dist/jquery.min.js\";\r\nimport \"bootstrap/dist/css/bootstrap.min.css\";\r\nimport \"datatables.net-dt/js/dataTables.dataTables\";\r\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\r\nimport $ from \"jquery\";\r\n\r\n\r\n\r\n\r\n// DataTable.use(DataTablesCore);\r\nexport default {\r\n    components: {\r\n        // DataTable,\r\n\r\n\r\n    },\r\n\r\n\r\n    data() {\r\n        const storageData = JSON.parse(localStorage.getItem('user'));\r\n\r\n        return {\r\n            backend_url: process.env.VUE_APP_API_URL,\r\n            storageData,\r\n            user: null,\r\n            departments: null,\r\n            designations: null,\r\n            transferType: null,\r\n            office: null,\r\n            edit: false,\r\n            url: this.backendUrl + \"/api/users\",\r\n            basicInfo: {\r\n                name: '',\r\n                mobileNumber: '',\r\n                email: '',\r\n                designation: '',\r\n                designation_id: '',\r\n                image: '',\r\n            },\r\n            transferRecordById: [],\r\n            promotionList1: [],\r\n            //     \"employee_id\": 1,\r\n            //   \"employee_name\": \"Shakiur\",\r\n            //   \"id\": 3,\r\n            //   \"name\": null,\r\n            //   \"promotion_ref_number\": \"sdf\",\r\n            //   \"promoted_designation\": 22,\r\n            //   \"promotion_date\": \"2023-03-28\",\r\n            //   \"description\": \"sfd\",\r\n            //   \"status\": 1,\r\n            //   \"created_at\": \"2023-10-30T08:19:36.000000Z\",\r\n            //   \"updated_at\": \"2023-10-30T08:23:44.000000Z\"\r\n            promotion: {\r\n                employee_id: null,\r\n                transfer_type: null,\r\n                transfer_order: null,\r\n                transfer_order_number: null,\r\n                to_office: null,\r\n                from_office: null,\r\n                to_department: null,\r\n                from_department: null,\r\n                to_designation: null,\r\n                from_designation: null,\r\n                transfer_date: null,\r\n                join_date: null,\r\n                transfer_letter: null\r\n            }\r\n\r\n        };\r\n    },\r\n\r\n    computed: {},\r\n\r\n    methods: {\r\n\r\n        editbutton(id) {\r\n            console.log(id);\r\n            this.axios\r\n                .get(this.backend_url + 'specificUserTransferRecord/' + id)\r\n                .then((response) => {\r\n                    this.transferRecordById = response.data.data;\r\n                    console.log(this.transferRecordById);\r\n                    this.promotion.id = id;\r\n                    this.promotion.employee_name = this.transferRecordById.employee_name;\r\n                    this.promotion.employee_id = this.transferRecordById.employee_id;\r\n                    this.promotion.transfer_type = this.transferRecordById.transfer_type;\r\n                    this.promotion.transfer_order = this.transferRecordById.transfer_order;\r\n                    this.promotion.transfer_order_number = this.transferRecordById.transfer_order_number;\r\n                    this.promotion.to_office = this.transferRecordById.to_office;\r\n                    this.promotion.from_office = this.transferRecordById.from_office;\r\n                    this.promotion.to_designation = this.transferRecordById.to_designation;\r\n                    this.promotion.from_designation = this.transferRecordById.from_designation;\r\n                    this.promotion.transfer_date = this.transferRecordById.transfer_date;\r\n                    this.promotion.join_date = this.transferRecordById.join_date;\r\n                    this.promotion.transfer_letter = this.transferRecordById.transfer_letter;\r\n                    this.promotion.to_department = this.transferRecordById.to_department;\r\n                    this.promotion.from_department = this.transferRecordById.from_department;\r\n\r\n\r\n                })\r\n                .catch((e) => {\r\n                    console.log(e)\r\n                })\r\n        },\r\n        deletebutton(id) {\r\n            this.axios\r\n                .delete(this.backend_url + 'deleteTransferRecord/' + id)\r\n                .then((res) => {\r\n                    console.log('Delete Successfully', res);\r\n                    this.getPromotionList();\r\n                    $('#datatable').DataTable().destroy();\r\n                    this.$toast.error(`Delete Successfully!!`);\r\n\r\n                })\r\n        },\r\n        viewDetails(id) {\r\n            console.log(id);\r\n            this.axios\r\n                .get(this.backend_url + 'specificUserTransferRecord/' + id)\r\n                .then((response) => {\r\n                    console.log(response);\r\n                    this.transferRecordById = response.data.data;\r\n                    console.log(this.transferRecordById.transfer_type);\r\n                    this.promotion.id = id;\r\n                    this.promotion.employee_name = this.transferRecordById.employee_name;\r\n                    this.promotion.employee_id = this.transferRecordById.employee_id;\r\n                    this.promotion.transfer_type = this.transferRecordById.transfer_type;\r\n                    this.promotion.transfer_order = this.transferRecordById.transfer_order;\r\n                    this.promotion.transfer_order_number = this.transferRecordById.transfer_order_number;\r\n                    this.promotion.to_office = this.transferRecordById.to_office;\r\n                    this.promotion.from_office = this.transferRecordById.from_office;\r\n                    this.promotion.to_designation = this.transferRecordById.to_designation;\r\n                    this.promotion.from_designation = this.transferRecordById.from_designation;\r\n                    this.promotion.transfer_date = this.transferRecordById.transfer_date;\r\n                    this.promotion.join_date = this.transferRecordById.join_date;\r\n                    this.promotion.transfer_letter = this.transferRecordById.transfer_letter;\r\n                    this.promotion.to_department = this.transferRecordById.to_department;\r\n                    this.promotion.from_department = this.transferRecordById.from_department;\r\n\r\n                    this.edit = false;\r\n                })\r\n                .catch((e) => {\r\n                    console.log(e)\r\n                })\r\n        },\r\n        getPromotionList() {\r\n            this.axios\r\n                .get(this.backend_url + `users/${this.storageData.id}`)\r\n                .then((res) => {\r\n                    this.user = res.data.userDetail.employee_id;\r\n                    // console.log(this.user);\r\n              