eeList() {\n      this.axios.get(this.backend_url + 'employeeList').then(res => {\n        console.log(res.data.employeeList);\n        this.employeeList = res.data.employeeList;\n        //   this.options =  res.data.employeeList;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list;\n        // console.log(this.office);\n      });\n    },\n\n    SendSMS() {\n      console.log(\"hi\");\n      this.axios.post(this.backend_url + 'sendMessage', this.sendSMS).then(res => {\n        console.log(res);\n        this.$toast.success(`Send SMS Successfully!!`);\n        this.clear();\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getDepartmentList() {\n      this.axios.get(this.backend_url + 'getDepartment').then(res => {\n        // console.log(res.data.data);\n        this.departments = res.data.data;\n      });\n    }\n  },\n  created() {\n    this.getDesignationList();\n    this.getOffice();\n    this.getDepartmentList();\n    this.getEmployeeList();\n  },\n  watch: {}\n};","map":{"version":3,"mappings":"AAuHA,OAAO,2BAA2B;AAClC,OAAO,sCAAsC;AAC7C,OAAO,4CAA4C;AACnD,OAAO,iDAAiD;AACxD;AACA,OAAO,KAAKA,GAAE,MAAO,KAAK;AAC1B,OAAOC,WAAU,MAAO,iBAAiB;;AAEzC;;AAEA;AACA,eAAe;EACXC,UAAU,EAAE;IACRD;EACJ,CAAC;EAEDE,IAAIA,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;IAE5D,MAAMC,MAAK,GAAIT,GAAG,CAACU,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC,CAElC,CAAC,CAAC;IAGF,OAAO;MACHF,MAAM;MACNG,WAAW,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAe;MACxCX,WAAW;MACXY,IAAI,EAAE,EAAE;MACRC,GAAG,EAAE,IAAI,CAACC,UAAS,GAAI,YAAY;MACnCC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE;QAAEC,EAAE,EAAE,EAAE;QAAEC,GAAG,EAAE;MAAG,CAAC;MAC1BC,WAAW,EAAE,EAAE;MACf;MACA;MACA;MACA;MACA;MACA;MACAC,MAAM,EAAE,EAAE;MACVJ,KAAK,EAAE;QAAEC,EAAE,EAAE,EAAE;QAAEC,GAAG,EAAE;MAAG,CAAC;MAC1BG,YAAY,EAAE,EAAE;MAChBC,OAAO,EAAE;QACLC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE,