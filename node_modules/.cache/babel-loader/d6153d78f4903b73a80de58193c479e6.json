s.storageData.id).then(res => {\n        console.log(res.data);\n        this.LeaveList = res.data.list;\n        setTimeout(function () {\n          $(\"#datatable\").DataTable({\n            \"processing\": true\n          });\n        }, 100);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getleaveList() {\n      this.axios.get(this.backend_url + 'getleave').then(res => {\n        // console.log(res.data.data);\n        this.leaves = res.data.data;\n      });\n    },\n    getLeavetype() {\n      this.axios.get(this.backend_url + 'LeaveType').then(res => {\n        this.LeaveType = res.data.list;\n        // console.log('fddsfsdf');\n        // console.log(this.LeaveType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id;\n        // console.log(this.user);\n        // this.leave.employee_id = this.user\n        // console.log(this.storageData.id);\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data;\n        // console.log(this.designations);\n      });\n    },\n\n    getleave() {\n      this.axios.get(this.backend_url + `getleaveMgt`).then(response => {\n        this.leave = response.data.list;\n        // console.log(this.leave);\n      });\n    },\n\n    activestatus(id) {\n      this.axios.patch(this.backend_url + `activeLeaveTypeRecord/${id}`).then(response => {\n        console.log(response);\n        this.getLeaveList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.success(`Active status Successfully!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    inactivestatus(id) {\n      this.axios.patch(this.backend_url + `inactiveLeaveTypeRecord/${id}`).then(response => {\n        console.log(response);\n        this.getLeaveList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(`Status Cancel!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    saveTranfer() {\n      try {\n        // this.leave.employee_id = this.storageData.id;\n        this.edit = false;\n        this.axios.post(this.backend_url + 'addLeaveType', this.leave).then(res => {\n          console.log(res);\n          this.getLeaveList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Add Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while saving the Leave record:\", error);\n          this.$toast.error(`Failed to save Leave record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to save Leave record.`);\n      }\n    }\n  },\n  created() {\n    this.getLeaveList();\n    this.getleaveList();\n    this.getDesignationList();\n    this.getLeavetype();\n    this.getUser();\n    // this.getleave();\n  },\n\n  watch: {}\n};","map":{"version":3,"mappings":"AAoMA,OAAO,2BAA2B;AAClC,OAAO,sCAAsC;AAC7C,OAAO,4CAA4C;AACnD,OAAO,iDAAiD;AACxD,OAAOA,OAAO,QAAQ;AACtB,SAASC,IAAI,EAAEC,KAAI,QAAS,cAAc;AAC1C,OAAO,KAAKC,GAAE,MAAO,KAAK;;AAE1B;AACA,eAAe;EACXC,UAAU,EAAE;IACR;IACAH,IAAI;IAAEC;EACV,CAAC;EAEDG,IAAIA,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;IAE5D,MAAMC,MAAK,GAAIR,GAAG,CAACS,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC;MAC9B;MACA;MACA;MACAC,UAAU,EAAEX,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,wBAAwB,CAAC;MAC3DC,IAAI,EAAEd,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,wBAAwB,CAAC;MACrDE,MAAM,EAAEf,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,oBAAoB;IACtD,CAAC,CAAC;IAGF,OAAO;MACHL,MAAM;MACNQ,WAAW,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAe;MACxChB,WAAW;MACXiB,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,EAAE;MACVC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,KAAK;MACXC