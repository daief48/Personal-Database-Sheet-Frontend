{"ast":null,"code":"import \"jquery/dist/jquery.min.js\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"datatables.net-dt/js/dataTables.dataTables\";\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\nimport $ from \"jquery\";\nimport { Form, Field } from 'vee-validate';\nimport * as Yup from \"yup\"; // DataTable.use(DataTablesCore);\n\nexport default {\n  components: {\n    // DataTable\n    Form,\n    Field\n  },\n\n  data() {\n    const storageData = JSON.parse(localStorage.getItem('user'));\n    const schema = Yup.object().shape({\n      leave_type: Yup.string().required(\"Office Name is required\"),\n      status: Yup.string().required(\"Status is required\")\n    });\n    return {\n      schema,\n      backend_url: process.env.VUE_APP_API_URL,\n      storageData,\n      user: \"\",\n      offices: \"\",\n      designations: \"\",\n      transferType: \"\",\n      edit: false,\n      url: this.backendUrl + \"/api/users\",\n      basicInfo: {\n        name: '',\n        mobileNumber: '',\n        email: '',\n        designation: '',\n        designation_id: '',\n        image: ''\n      },\n      transferRecordById: [],\n      transferList: [],\n      office: {\n        leave_type: \"\",\n        days: \"\",\n        status: \"\"\n      }\n    };\n  },\n\n  computed: {},\n  methods: {\n    clear() {\n      this.office.leave_type = \"\";\n      this.office.status = \"\";\n    },\n\n    getTransferList() {\n      this.axios.get(this.backend_url + 'getLeaveType?user_id=' + this.storageData.id).then(res => {\n        console.log(res.data);\n        this.transferList = res.data.list;\n        setTimeout(function () {\n          $(\"#datatable\").DataTable({\n            \"processing\": true\n          });\n        }, 100);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    getofficeList() {\n      this.axios.get(this.backend_url + 'getoffice').then(res => {\n        // console.log(res.data.data);\n        this.offices = res.data.data;\n      });\n    },\n\n    getTransfertype() {\n      this.axios.get(this.backend_url + 'transferType').then(res => {\n        this.transferType = res.data.list; // console.log('fddsfsdf');\n        // console.log(this.transferType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n        // this.office.employee_id = this.user\n        // console.log(this.storageData.id);\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data; // console.log(this.designations);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list; // console.log(this.office);\n      });\n    },\n\n    activestatus(id) {\n      this.axios.patch(this.backend_url + `activeLeaveTypeRecord/${id}`).then(response => {\n        console.log(response);\n        this.getTransferList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.success(`Active status Successfully!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n\n    inactivestatus(id) {\n      this.axios.patch(this.backend_url + `inactiveLeaveTypeRecord/${id}`).then(response => {\n        console.log(response);\n        this.getTransferList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(`Status Cancel!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n\n    saveTranfer() {\n      try {\n        // this.office.employee_id = this.storageData.id;\n        this.edit = false;\n        this.axios.post(this.backend_url + 'addLeaveType', this.office).then(res => {\n          console.log(res);\n          this.getTransferList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Add Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while saving the transfer record:\", error);\n          this.$toast.error(`Failed to save transfer record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to save transfer record.`);\n      }\n    }\n\n  },\n\n  created() {\n    this.getTransferList();\n    this.getofficeList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser(); // this.getOffice();\n  },\n\n  watch: {}\n};","map":{"version":3,"mappings":"AAoNA,OAAO,2BAAP;AACA,OAAO,sCAAP;AACA,OAAO,4CAAP;AACA,OAAO,iDAAP;AACA,OAAOA,CAAP,MAAc,QAAd;AACA,SAASC,IAAT,EAAeC,KAAf,QAA4B,cAA5B;AACA,OAAO,KAAKC,GAAZ,MAAqB,KAArB,C,CAEA;;AACA,eAAe;EACXC,UAAU,EAAE;IACR;IACAH,IAFQ;IAEFC;EAFE,CADD;;EAMXG,IAAI,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX,CAApB;IAEA,MAAMC,MAAK,GAAIR,GAAG,CAACS,MAAJ,GAAaC,KAAb,CAAmB;MAC9BC,UAAU,EAAEX,GAAG,CAACY,MAAJ,GAAaC,QAAb,CAAsB,yBAAtB,CADkB;MAE9BC,MAAM,EAAEd,GAAG,CAACY,MAAJ,GAAaC,QAAb,CAAsB,oBAAtB;IAFsB,CAAnB,CAAf;IAMA,OAAO;MACHL,MADG;MAEHO,WAAW,EAAEC,OAAO,CAACC,GAAR,CAAYC,eAFtB;MAGHf,WAHG;MAIHgB,IAAI,EAAE,EAJH;MAKHC,OAAO,EAAE,EALN;MAMHC,YAAY,EAAE,EANX;MAOHC,YAAY,EAAE,EAPX;MAQHC,IAAI,EAAE,KARH;MASHC,GAAG,EAAE,KAAKC,UAAL,GAAkB,YATpB;MAUHC,SAAS,EAAE;QACPC,IAAI,EAAE,EADC;QAEPC,YAAY,EAAE,EAFP;QAGPC,KAAK,EAAE,EAHA;QAIPC,WAAW,EAAE,EAJN;QAKPC,cAAc,EAAE,EALT;QAMPC,KAAK,EAAE;MANA,CAVR;MAkBHC,kBAAkB,EAAE,EAlBjB;MAmBHC,YAAY,EAAE,EAnBX;MAoBHC,MAAM,EAAE;QACJxB,UAAU,EAAE,EADR;QAEJyB,IAAI,EAAC,EAFD;QAGJtB,MAAM,EAAE;MAHJ;IApBL,CAAP;EA2BH,CA1CU;;EA4CXuB,QAAQ,EAAE,EA5CC;EA8CXC,OAAO,EAAE;IACLC,KAAK,GAAG;MACJ,KAAKJ,MAAL,CAAYxB,UAAZ,GAAyB,EAAzB;MACA,KAAKwB,MAAL,CAAYrB,MAAZ,GAAqB,EAArB;IACH,CAJI;;IAKL0B,eAAe,GAAG;MACd,KAAKC,KAAL,CACKC,GADL,CACS,KAAK3B,WAAL,GAAmB,uBAAnB,GA