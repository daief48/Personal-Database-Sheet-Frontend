                        </button>\r\n                                        </div>\r\n\r\n\r\n                                    </td>\r\n\r\n                                </tr>\r\n\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- Modal Start -->\r\n\r\n\r\n\r\n    </div>\r\n</template>\r\n    \r\n<script>\r\n\r\nimport \"jquery/dist/jquery.min.js\";\r\nimport \"bootstrap/dist/css/bootstrap.min.css\";\r\nimport \"datatables.net-dt/js/dataTables.dataTables\";\r\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\r\nimport $ from \"jquery\";\r\n// DataTable.use(DataTablesCore);\r\nexport default {\r\n    components: {\r\n        // DataTable,\r\n\r\n    },\r\n\r\n\r\n    data() {\r\n        const storageData = JSON.parse(localStorage.getItem('user'));\r\n\r\n        return {\r\n            backend_url: process.env.VUE_APP_API_URL,\r\n            storageData,\r\n            user: null,\r\n            departments: null,\r\n            designations: null,\r\n            transferType: null,\r\n            office: null,\r\n            edit: false,\r\n            url: this.backendUrl + \"/api/users\",\r\n            basicInfo: {\r\n                name: '',\r\n                mobileNumber: '',\r\n                email: '',\r\n                designation: '',\r\n                designation_id: '',\r\n                image: '',\r\n            },\r\n            transferRecordById: [],\r\n            trainingList1: [],\r\n            //         <!-- \"employee_id\": 1,\r\n            //   \"employee_name\": \"Shakiur\",\r\n            //   \"id\": 2,\r\n            //   \"training_center_name\": \"322\",\r\n            //   \"training_name\": \"322\",\r\n            //   \"training_score\": 322,\r\n            //   \"training_feedback\": \"322\",\r\n            //   \"training_strt_date\": \"2023-10-29\",\r\n            //   \"training_end_date\": \"2023-10-29\",\r\n            //   \"description\": \"322\",\r\n            //   \"status\": 0,\r\n            //   \"created_at\": \"2023-10-29T06:30:12.000000Z\",\r\n            //   \"updated_at\": \"2023-10-29T06:42:12.000000Z\" -->\r\n            training: {\r\n                employee_id: null,\r\n                training_center_name: null,\r\n                training_name: null,\r\n                training_score: null,\r\n                training_feedback: null,\r\n                training_strt_date: null,\r\n                training_end_date: null,\r\n            }\r\n\r\n        };\r\n    },\r\n\r\n    computed: {},\r\n\r\n    methods: {\r\n\r\n        editbutton(id) {\r\n            console.log(id);\r\n            // this.axios\r\n            //     .get(this.backend_url + 'specificUserTransferRecord/' + id)\r\n            //     .then((response) => {\r\n            //         this.transferRecordById = response.data.data;\r\n            //         console.log(this.transferRecordById);\r\n            //         this.training.id = id;\r\n            //         this.training.employee_name = this.transferRecordById.employee_name;\r\n            //         this.training.employee_id = this.transferRecordById.employee_id;\r\n            //         this.training.transfer_type = this.transferRecordById.transfer_type;\r\n            //         this.training.transfer_order = this.transferRecordById.transfer_order;\r\n            //         this.training.transfer_order_number = this.transferRecordById.transfer_order_number;\r\n            //         this.training.to_office = this.transferRecordById.to_office;\r\n            //         this.training.from_office = this.transferRecordById.from_office;\r\n            //         this.training.to_designation = this.transferRecordById.to_designation;\r\n            //         this.training.from_designation = this.transferRecordById.from_designation;\r\n            //         this.training.transfer_date = this.transferRecordById.transfer_date;\r\n            //         this.training.join_date = this.transferRecordById.join_date;\r\n            //         this.training.transfer_letter = this.transferRecordById.transfer_letter;\r\n            //         this.training.to_department = this.transferRecordById.to_department;\r\n            //         this.training.from_department = this.transferRecordById.from_department;\r\n\r\n            //         this.edit = true;\r\n            //     })\r\n            //     .catch((e) => {\r\n            //         console.log(e)\r\n            //     })\r\n        },\r\n        deletebutton(id) {\r\n            this.axios\r\n                .delete(this.backend_url + 'deleteTransferRecord/' + id)\r\n                .then((res) => {\r\n                    console.log('Delete Successfully', res);\r\n                    this.getTrainingList();\r\n                    $('#datatable').DataTable().destroy();\r\n\r\n                })\r\n        },\r\n        viewDetails(id) {\r\n            console.log(id);\r\n            this.axios\r\n                .get(this.backend_url + 'specificUserTransferRecord/' + id)\r\n                .then((response) => {\r\n                    console.log(response);\r\n                    this.transferRecordById = response.data.data;\r\n                    console.log(this.transferRecordById.transfer_type);\r\n                    this.training.id = id;\r\n                    this.training.employee_name = this.transferRecordById.employee_name;\r\n                    this.training.employee_id = this.transferRecordById.employee_id;\r\n                    this.training.transfer_type = this.transferRecordById.transfer_type;\r\n                    this.training.transfer_order = this.transferRecordById.transfer_order;\r\n                    this.training.transfer_order_number = this.transferRecordById.transfer_order_number;\r\n                    this.training.to_office = this.transferRecordById.to_office;\r\n                    this.training.from_office = this.transferRecordById.from_office;\r\n                    this.training.to_designation = this.transferRecordById.to_designation;\r\n                    this.training.from_designation = this.transferRecordById.from_designation;\r\n                    this.training.transfer_date = this.transferRecordById.transfer_date;\r\n                    this.training.join_date = this.transferRecordById.join_date;\r\n                    this.training.transfer_letter = this.transferRecordById.transfer_letter;\r\n                    this.training.to_department = this.transferRecordById.to_department;\r\n                    this.training.from_department = this.transferRecordById.from_department;\r\n\r\n                    this.edit = false;\r\n                })\r\n                .catch((e) => {\r\n                    console.log(e)\r\n                })\r\n        },\r\n        getTrainingList() {\r\n            this.axios\r\n                .get(this.backend_url + `users/${this.storageData.id}`)\r\n                .then((res) => {\r\n                    this.user = res.data.userDetail.employee_id;\r\n                    // console.log(this.user);\r\n                    this.training.employee_id = this.user\r\n                    // console.log(this.storageData.id);\r\n\r\n                    this.axios\r\n                        .get(this.backend_url + 'specificUserTrainingRecordByEmployeeId/' + this.user)\r\n                        .then((response) => {\r\n                            this.trainingList1 = response.data;\r\n                            // Initialize DataTable here\r\n                            setTimeout(function () {\r\n                                $(\"#datatable\").DataTable({\r\n                                    \"processing\": true,\r\n                                });\r\n                            }, 100);\r\n                        })\r\n                        .catch((e) => {\r\n                            console.log(e);\r\n                        });\r\n\r\n                })\r\n\r\n        },\r\n\r\n        getDepartmentList() {\r\n            this.axios\r\n                .get(this.backend_url + 'getDepartment')\r\n                .then((res) => {\r\n                    // console.log(res.data.data);\r\n                    this.departments = res.data.data;\r\n\r\n                })\r\n        },\r\n\r\n        getDesignationList() {\r\n            this.axios\r\n                .get(this.backend_url + 'getDesignationMgt')\r\n                .then((res) => {\r\n                    this.designations = res.data.data;\r\n                    // console.log(this.designations);\r\n                })\r\n        },\r\n        getTransfertype() {\r\n            this.axios\r\n                .get(this.backend_url + 'transferType')\r\n                .then((res) => {\r\n                    this.transferType = res.data.list;\r\n                    // console.log('fddsfsdf');\r\n                    // console.log(this.transferType[0].title);\r\n                })\r\n        },\r\n        getUser() {\r\n            this.axios\r\n                .get(this.backend_url + `users/${this.storageData.id}`)\r\n                .then((res) => {\r\n                    this.user = res.data.userDetail.employee_id;\r\n                    // console.log(this.user);\r\n                    this.training.employee_id = this.user\r\n                    // console.log(this.storageData.id);\r\n                })\r\n        },\r\n        getOffice() {\r\n            this.axios\r\n                .get(this.backend_url + `getOfficeMgt`)\r\n                .then((response) => {\r\n                    this.office = response.data.list;\r\n                    // console.log(this.office);\r\n                })\r\n        },\r\n        saveTraining() {\r\n            // this.training.employee_id = this.storageData.id;\r\n            this.edit = false;\r\n            this.axios\r\n                .post(this.backend_url + 'addTrainingRecord', this.training)\r\n                .then((res) => {\r\n                    console.log(res);\r\n\r\n                    $(\"#exampleModal\").modal('hide');\r\n\r\n                })\r\n            this.getTrainingList();\r\n            $('#datatable').DataTable().destroy();\r\n        },\r\n        EditTranfer() {\r\n            this.axios\r\n                .post(this.backend_url + `updateTransferRecord/${this.training.id}`, this.training)\r\n                .then((res) => {\r\n                    console.log(res);\r\n                    this.getTrainingList();\r\n                    $(\"#exampleModal\").modal('hide');\r\n                })\r\n        }\r\n\r\n    },\r\n\r\n    created() {\r\n        this.getTrainingList();\r\n        this.getDepartmentList();\r\n        this.getDesignationList();\r\n        this.getTransfertype();\r\n        this.getUser();\r\n        this.getOffice();\r\n    },\r\n    mounted() {\r\n\r\n\r\n    },\r\n\r\n\r\n    watch: {\r\n\r\n    },\r\n\r\n}\r\n\r\n\r\n</script>\r\n    \r\n<style>\r\n.card-title {\r\n    /* text-align: center; */\r\n    font-size: 20px;\r\n    font-weight: bolder;\r\n}\r\n\r\n.ui.inverted.dimmer {\r\n    color: #009688 !important;\r\n\r\n}\r\n\r\n.cropper {\r\n    height: 400px;\r\n    width: 400px;\r\n    background: #DDD;\r\n}\r\n\r\n\r\n.file-btn {\r\n    position: relative;\r\n    display: block;\r\n    cursor: pointer;\r\n}\r\n\r\n.file-btn input {\r\n    position: absolute;\r\n    height: 100%;\r\n    width: 100%;\r\n    left: 0;\r\n    top: 0;\r\n    opacity: 0;\r\n    cursor: pointer;\r\n}\r\n</style>\r\n    \r\n<style scoped>\r\n::v-deep .modal-container {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n::v-deep .modal-content {\r\n    position: relative;\r\n    display: flex;\r\n    flex-direction: column;\r\n    margin: 0 1rem;\r\n    padding: 1rem;\r\n    border: 1px solid #e2e8f0;\r\n    border-radius: 0.25rem;\r\n    background: #fff;\r\n}\r\n\r\n.modal__title {\r\n    margin: 0 2rem 0 0;\r\n    font-size: 1.5rem;\r\n    font-weight: 700;\r\n}\r\n\r\n.modal__close {\r\n    position: absolute;\r\n    top: 0.5rem;\r\n    right: 0.5rem;\r\n}\r\n</style>\r\n      \r\n<style scoped>\r\n.dark-mode div::v-deep .modal-content {\r\n    border-color: #2d3748;\r\n    background-color: #1a202c;\r\n}</style>"]},"metadata":{},"sourceType":"module"}