ById = response.data.data;\n      //         console.log(this.transferRecordById);\n      //         this.training.id = id;\n      //         this.training.employee_name = this.transferRecordById.employee_name;\n      //         this.training.employee_id = this.transferRecordById.employee_id;\n      //         this.training.transfer_type = this.transferRecordById.transfer_type;\n      //         this.training.transfer_order = this.transferRecordById.transfer_order;\n      //         this.training.transfer_order_number = this.transferRecordById.transfer_order_number;\n      //         this.training.to_office = this.transferRecordById.to_office;\n      //         this.training.from_office = this.transferRecordById.from_office;\n      //         this.training.to_designation = this.transferRecordById.to_designation;\n      //         this.training.from_designation = this.transferRecordById.from_designation;\n      //         this.training.transfer_date = this.transferRecordById.transfer_date;\n      //         this.training.join_date = this.transferRecordById.join_date;\n      //         this.training.transfer_letter = this.transferRecordById.transfer_letter;\n      //         this.training.to_department = this.transferRecordById.to_department;\n      //         this.training.from_department = this.transferRecordById.from_department;\n      //         this.edit = true;\n      //     })\n      //     .catch((e) => {\n      //         console.log(e)\n      //     })\n    },\n\n    deletebutton(id) {\n      this.axios.delete(this.backend_url + 'deleteTransferRecord/' + id).then(res => {\n        console.log('Delete Successfully', res);\n        this.getTrainingList();\n        $('#datatable').DataTable().destroy();\n      });\n    },\n\n    viewDetails(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificUserTransferRecord/' + id).then(response => {\n        console.log(response);\n        this.transferRecordById = response.data.data;\n        console.log(this.transferRecordById.transfer_type);\n        this.training.id = id;\n        this.training.employee_name = this.transferRecordById.employee_name;\n        this.training.employee_id = this.transferRecordById.employee_id;\n        this.training.transfer_type = this.transferRecordById.transfer_type;\n        this.training.transfer_order = this.transferRecordById.transfer_order;\n        this.training.transfer_order_number = this.transferRecordById.transfer_order_number;\n        this.training.to_office = this.transferRecordById.to_office;\n        this.training.from_office = this.transferRecordById.from_office;\n        this.training.to_designation = this.transferRecordById.to_designation;\n        this.training.from_designation = this.transferRecordById.from_designation;\n        this.training.transfer_date = this.transferRecordById.transfer_date;\n        this.training.join_date = this.transferRecordById.join_date;\n        this.training.transfer_letter = this.transferRecordById.transfer_letter;\n        this.training.to_department = this.transferRecordById.to_department;\n        this.training.from_department = this.transferRecordById.from_department;\n        this.edit = false;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    getTrainingList() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n\n        this.training.employee_id = this.user; // console.log(this.storageData.id);\n\n        this.axios.get(this.backend_url + 'specificUserTrainingRecordByEmployeeId/' + this.user).then(response => {\n          this.trainingList1 = response.data; // Initialize DataTable here\n\n          setTimeout(function () {\n            $(\"#datatable\").DataTable({\n              \"processing\": true\n            });\n          }, 100);\n        }).catch(e => {\n          console.log(e);\n        });\n      });\n    },\n\n    getDepartmentList() {\n      this.axios.get(this.backend_url + 'getDepartment').then(res => {\n        // console.log(res.data.data);\n        this.departments = res.data.data;\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data; // console.log(this.designations);\n      });\n    },\n\n    getTransfertype() {\n      this.axios.get(this.backend_url + 'transferType').then(res => {\n        this.transferType = res.data.list; // console.log('fddsfsdf');\n        // console.log(this.transferType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n\n        this.training.employee_id = this.user; // console.log(this.storageData.id);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list; // console.log(this.office);\n      });\n    },\n\n    saveTraining() {\n      // this.training.employee_id = this.storageData.id;\n      this.edit = false;\n      this.axios.post(this.backend_url + 'addTrainingRecord', this.training).then(res => {\n        console.log(res);\n        $(\"#exampleModal\").modal('hide');\n      });\n      this.getTrainingList();\n      $('#datatable').DataTable().destroy();\n    },\n\n    EditTranfer() {\n      this.axios.post(this.backend_url + `updateTransferRecord/${this.training.id}`, this.training).then(res => {\n        console.log(res);\n        this.getTrainingList();\n        $(\"#exampleModal\").modal('hide');\n      });\n    }\n\n  },\n\n  created() {\n    this.getTrainingList();\n    this.getDepartmentList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser();\n    this.getOffice();\n  },\n\n  mounted() {},\n\n  watch: {}\n};","map":{"ver