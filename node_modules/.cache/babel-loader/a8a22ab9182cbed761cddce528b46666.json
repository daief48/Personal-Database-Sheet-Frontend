ption>\r\n                                            <!-- Loop through departments -->\r\n                                            <option v-for=\"department in departments\" :key=\"department.id\"\r\n                                                :value=\"department.id\">\r\n                                                {{ department.dept_name }}\r\n                                            </option>\r\n                                        </select>\r\n                                    </div>\r\n\r\n\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n                            <div class=\"row mb-3 align-items-center\">\r\n                                <div class=\"w-25\">\r\n                                    <label for=\"exampleFormControlSelect1\">Office</label>\r\n                                </div>\r\n                                <div class=\"w-75\">\r\n                                    <div class=\"form-group mr-2\">\r\n                                        <select class=\"form-control\" id=\"exampleFormControlSelect1\" name=\"from_office\"\r\n                                            v-model=\"sendSMS.offices\" @change=\"changeHH\">\r\n                                            <option value=\"\" disabled selected>Select an office</option>\r\n\r\n                                            <option v-bind:value=\"office.id\" v-for=\"office in office\" :key=\"office.id\">\r\n                                                {{ office.office_name }}\r\n                                            </option>\r\n\r\n                                        </select>\r\n                                    </div>\r\n\r\n\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n                            <div class=\"row mb-3 align-items-center\">\r\n                                <div class=\"w-25\">\r\n                                    <label for=\"exampleFormCo