{"ast":null,"code":"import DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net';\nDataTable.use(DataTablesCore);\nexport default {\n  components: {\n    DataTable\n  },\n\n  data() {\n    const storageData = JSON.parse(localStorage.getItem('user'));\n    const columns = [{\n      data: 'id'\n    }, {\n      data: 'employee_name'\n    }, {\n      data: 'to_department'\n    }, {\n      data: 'from_department'\n    }, {\n      data: 'designation'\n    }, {\n      data: 'transfer_type'\n    }, {\n      data: 'transfer_order'\n    }, {\n      data: 'to_office'\n    }, {\n      data: 'from_office'\n    }, {\n      data: 'transfer_date'\n    }, {\n      data: 'join_date'\n    }, {\n      title: 'Actions',\n      orderable: false,\n      searchable: false,\n\n      render(data, type, row) {\n        return `\n            <button class=\"btn btn-primary btn-sm\" @click=\"editItem(${row.id})\">Edit</button>\n            <button class=\"btn btn-danger btn-sm\" @click=\"deleteItem(${row.id})\">Delete</button>\n          `;\n      }\n\n    }];\n    return {\n      backend_url: process.env.VUE_APP_API_URL,\n      storageData,\n      user: null,\n      departments: null,\n      designations: null,\n      transferType: null,\n      url: this.backendUrl + \"/api/users\",\n      basicInfo: {\n        name: '',\n        mobileNumber: '',\n        email: '',\n        designation: '',\n        designation_id: '',\n        image: ''\n      },\n      columns,\n      transferList: [],\n      tansfer: {\n        employee_id: null,\n        transfer_type: null,\n        transfer_order: null,\n        transfer_order_number: null,\n        to_office: null,\n        from_office: null,\n        designation: null,\n        department: null,\n        transfer_date: null,\n        join_date: null,\n        transfer_letter: null\n      }\n    };\n  },\n\n  computed: {},\n  methods: {\n    getTransferList() {\n      this.axios.get(this.backend_url + 'getTransferList?user_id=' + this.storageData.id).then(res => {\n        // console.log(res.data.data);\n        this.transferList = res.data.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    getDepartmentList() {\n      this.axios.get(this.backend_url + 'getDepartment').then(res => {\n        // console.log(res.data.data);\n        this.departments = res.data.data;\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data; // console.log(this.designations);\n      });\n    },\n\n    getTransfertype() {\n      this.axios.get(this.backend_url + 'getTransferList').then(res => {\n        this.transferType = res.data.data; // console.log(this.transferType);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.tansfer.employee_id = res.data.userDetail.employee_id; // console.log(this.user.employee_id);\n\n        console.log(this.storageData.id);\n      });\n    },\n\n    saveTranfer() {\n      // this.tansfer.employee_id = this.storageData.id;\n      this.axios.post(this.backend_url + 'addTransferRecord', this.tansfer).then(res => {\n        console.log(res.data);\n        this.getProfileInfo();\n      });\n    }\n\n  },\n\n  created() {\n    this.getTransferList();\n    this.getDepartmentList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser();\n  },\n\n  watch: {}\n};","map":{"version":3,"mappings":"AAuKA,OAAOA,SAAP,MAAsB,qBAAtB;AACA,OAAOC,cAAP,MAA2B,gBAA3B;AACAD,SAAS,CAACE,GAAV,CAAcD,cAAd;AACA,eAAe;EACbE,UAAU,EAAE;IACVH;EADU,CADC;;EAKbI,IAAI,GAAG;IACL,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX,CAApB;IACA,MAAMC,OAAM,GAAI,CACd;MAAEN,IAAI,EAAE;IAAR,CADc,EAEd;MAAEA,IAAI,EAAE;IAAR,CAFc,EAGd;MAAEA,IAAI,EAAE;IAAR,CAHc,EAId;MAAEA,IAAI,EAAE;IAAR,CAJc,EAKd;MAAEA,IAAI,EAAE;IAAR,CALc,EAMd;MAAEA,IAAI,EAAE;IAAR,CANc,EAO