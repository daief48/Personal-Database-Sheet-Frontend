{"ast":null,"code":"import \"jquery/dist/jquery.min.js\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"datatables.net-dt/js/dataTables.dataTables\";\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\nimport $ from \"jquery\"; // DataTable.use(DataTablesCore);\n\nexport default {\n  components: {// DataTable,\n  },\n\n  data() {\n    const storageData = JSON.parse(localStorage.getItem('user'));\n    return {\n      backend_url: process.env.VUE_APP_API_URL,\n      storageData,\n      user: null,\n      departments: null,\n      designations: null,\n      transferType: null,\n      office: null,\n      edit: false,\n      url: this.backendUrl + \"/api/users\",\n      basicInfo: {\n        name: '',\n        mobileNumber: '',\n        email: '',\n        designation: '',\n        designation_id: '',\n        image: ''\n      },\n      transferRecordById: [],\n      promotionList1: [],\n      //     \"employee_id\": 1,\n      //   \"employee_name\": \"Shakiur\",\n      //   \"id\": 3,\n      //   \"name\": null,\n      //   \"promotion_ref_number\": \"sdf\",\n      //   \"promoted_designation\": 22,\n      //   \"promotion_date\": \"2023-03-28\",\n      //   \"description\": \"sfd\",\n      //   \"status\": 1,\n      //   \"created_at\": \"2023-10-30T08:19:36.000000Z\",\n      //   \"updated_at\": \"2023-10-30T08:23:44.000000Z\"\n      promotion: {\n        employee_id: null,\n        transfer_type: null,\n        transfer_order: null,\n        transfer_order_number: null,\n        to_office: null,\n        from_office: null,\n        to_department: null,\n        from_department: null,\n        to_designation: null,\n        from_designation: null,\n        transfer_date: null,\n        join_date: null,\n        transfer_letter: null\n      }\n    };\n  },\n\n  computed: {},\n  methods: {\n    editbutton(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificUserTransferRecord/' + id).then(response => {\n        this.transferRecordById = response.data.data;\n        console.log(this.transferRecordById);\n        this.promotion.id = id;\n        this.promotion.employee_name = this.transferRecordById.employee_name;\n        this.promotion.employee_id = this.transferRecordById.employee_id;\n        this.promotion.transfer_type = this.transferRecordById.transfer_type;\n        this.promotion.transfer_order = this.transferRecordById.transfer_order;\n        this.promotion.transfer_order_number = this.transferRecordById.transfer_order_number;\n        this.promotion.to_office = this.transferRecordById.to_office;\n        this.promotion.from_office = this.transferRecordById.from_office;\n        this.promotion.to_designation = this.transferRecordById.to_designation;\n        this.promotion.from_designation = this.transferRecordById.from_designation;\n        this.promotion.transfer_date = this.transferRecordById.transfer_date;\n        this.promotion.join_date = this.transferRecordById.join_date;\n        this.promotion.transfer_letter = this.transferRecordById.transfer_letter;\n        this.promotion.to_department = this.transferRecordById.to_department;\n        this.promotion.from_department = this.transferRecordById.from_department;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    deletebutton(id) {\n      this.axios.delete(this.backend_url + 'deleteTransferRecord/' + id).then(res => {\n        console.log('Delete Successfully', res);\n        this.getPromotionList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(`Delete Successfully!!`);\n      });\n    },\n\n    viewDetails(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificUserTransferRecord/' + id).then(response => {\n        console.log(response);\n        this.transferRecordById = response.data.data;\n        console.log(this.transferRecordById.transfer_type);\n        this.promotion.id = id;\n        this.promotion.employee_name = this.transferRecordById.employee_name;\n        this.promotion.employee_id = this.transferRecordById.employee_id;\n        this.promotion.transfer_type = this.transferRecordById.transfer_type;\n        this.promotion.transfer_order = this.transferRecordById.transfer_order;\n        this.promotion.transfer_order_number = this.transferRecordById.transfer_order_number;\n        this.promotion.to_office = this.transferRecordById.to_office;\n        this.promotion.from_office = this.transferRecordById.from_office;\n        this.promotion.to_designation = this.transferRecordById.to_designation;\n        this.promotion.from_designation = this.transferRecordById.from_designation;\n        this.promotion.transfer_date = this.transferRecordById.transfer_date;\n        this.promotion.join_date = this.transferRecordById.join_date;\n        this.promotion.transfer_letter = this.transferRecordById.transfer_letter;\n        this.promotion.to_department = this.transferRecordById.to_department;\n        this.promotion.from_department = this.transferRecordById.from_department;\n        this.edit = false;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    getPromotionList() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n\n        this.promotion.employee_id = this.user; // console.log(this.storageData.id);\n\n        this.axios.get(this.backend_url + 'specificUserPromotionRecordByEmployeeId/' + this.user).then(response => {\n          this.promotionList1 = response.data; // Initialize DataTable here\n\n          setTimeout(function () {\n            $(\"#datatable\").DataTable({\n              \"processing\": true\n            });\n          }, 100);\n        }).catch(e => {\n          console.log(e);\n        });\n      });\n    },\n\n    getDepartmentList() {\n      this.axios.get(this.backend_url + 'getDepartment').then(res => {\n        // console.log(res.data.data);\n        this.departments = res.data.data;\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data; // console.log(this.designations);\n      });\n    },\n\n    getTransfertype() {\n      this.axios.get(this.backend_url + 'transferType').then(res => {\n        this.transferType = res.data.list; // console.log('fddsfsdf');\n        // console.log(this.transferType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n\n        this.promotion.employee_id = this.user; // console.log(this.storageData.id);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list; // console.log(this.office);\n      });\n    },\n\n    savePromotion() {\n      try {\n        // this.promotion.employee_id = this.storageData.id;\n        this.edit = false;\n        this.axios.post(this.backend_url + 'addTransferRecord', this.promotion).then(res => {\n          console.log(res);\n          this.getPromotionList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Add Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while saving the transfer record:\", error);\n          this.$toast.error(`Failed to save transfer record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to save transfer record.`);\n      }\n    },\n\n    EditTranfer() {\n      try {\n        this.axios.post(this.backend_url + `updateTransferRecord/${this.promotion.id}`, this.promotion).then(res => {\n          console.log(res);\n          this.getPromotionList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Update Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while updating the transfer record:\", error);\n          this.$toast.error(`Failed to update transfer record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to update transfer record.`);\n      }\n    }\n\n  },\n\n  created() {\n    this.getPromotionList();\n    this.getDepartmentList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser();\n    this.getOffice();\n  },\n\n  mounted() {},\n\n  watch: {}\n};","map":{"version":3,"mappings":"AA4RE,OAAO,2BAAP;AACA,OAAO,sCAAP;AACA,OAAO,4CAAP;AACA,OAAO,iDAAP;AACA,OAAOA,CAAP,MAAc,QAAd,C,CAKA;;AACA,eAAe;EACbC,UAAU,EAAE,CACV;EADU,CADC;;EAQbC,IAAI,GAAG;IACL,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX,CAApB;IAEA,OAAO;MACLC,WAAW,EAAEC,OAAO,CAACC,GAAR,CAAYC,eADpB;MAELR,WAFK;MAGLS,IAAI,EAAE,IAHD;MAILC,WAAW,EAAE,IAJR;MAKLC,YAAY,EAAE,IALT;MAMLC,YAAY,EAAE,IANT;MAOLC,MAAM,EAAE,IAPH;MAQLC,IAAI,EAAE,KARD;MASLC,GAAG,EAAE,KAAKC,UAAL,GAAkB,YATlB;MAULC,SAAS,EAAE;QACTC,IAAI,EAAE,EADG;QAETC,YAAY,EAAE,EAFL;QAGTC,KAAK,EAAE,EAHE;QAITC,WAAW,EAAE,EAJJ;QAKTC,cAAc,EAAE,EALP;QAMTC,KAAK,EAAE;MANE,CAVN;MAkBLC,kBAAkB,EAAE,EAlBf;MAmBLC,cAAc,EAAE,EAnBX;MAoBT;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACIC,SAAS,EAAE;QACTC,WAAW,EAAE,IADJ;QAETC,aAAa,EAAE,IAFN;QAGTC,cAAc,EAAE,IAHP;QAITC,qBAAqB,EAAE,IAJd;QAKTC,SAAS,EAAE,IALF;QAMTC,WAAW,EAAE,IANJ;QAOTC,aAAa,EAAE,IAPN;QAQTC,eAAe,EAAE,IARR;QASTC,cAAc,EAAE,IATP;QAUTC,gBAAgB,EAAE,IAVT;QAWTC,aAAa,EAAE,IAXN;QAYTC,SAAS,EAAE,IAZF;QAaTC,eAAe,EAAE;MAbR;IA/BN,CAAP;EAgDD,CA3DY;;EA6DbC,QAAQ,EAAE,EA7DG;EA+DbC,OAAO,EAAE;IAEPC,UAAU,CAACC,EAAD,EAAK;MACbC,OAAO,CAACC,GAAR,CAAYF,EAAZ;MACA,KAAKG,KAAL,CACGC,GADH,CACO,KAAK1C,WAAL,GAA