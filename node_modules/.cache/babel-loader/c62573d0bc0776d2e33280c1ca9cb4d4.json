\n              </div>\r\n            </div>\r\n            <!--End Add and Edit Modal -->\r\n  \r\n  \r\n            <!-- Transter Details Modal -->\r\n  \r\n            <div class=\"modal fade\" id=\"exampleModal2\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLabel\"\r\n              aria-hidden=\"true\">\r\n              <div class=\"modal-dialog\" role=\"document\">\r\n                <div class=\"modal-content\" style=\"width: 121%; top: 164px;\">\r\n                  <div class=\"modal-header\">\r\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                      <span aria-hidden=\"true\">&times;</span>\r\n                    </button>\r\n                  </div>\r\n                  <div class=\"modal-body card\">\r\n                    <div class=\"card-body d-flex flex-column\">\r\n                      <h5 class=\"modal-title text-center\" id=\"exampleModalLabel\" style=\"font-size: 25px;\">Transfer Details\r\n                      </h5>\r\n                      <hr>\r\n                      <h1 class=\"card-title \">Transfer Type: {{ promotion.transfer_type }}</h1>\r\n                      <h5 class=\"card-title\">Transfer Order: {{ promotion.transfer_order }}</h5>\r\n                      <h5 class=\"card-title\">Transfer Order Number: {{ promotion.transfer_order_number }}</h5>\r\n                      <h5 class=\"card-title\">To Office: {{ promotion.to_office }}</h5>\r\n                      <h5 class=\"card-title\">From Office: {{ promotion.from_office }}</h5>\r\n                      <h5 class=\"card-title\">To Designation: {{ promotion.to_designation }}</h5>\r\n                      <h5 class=\"card-title\">From Designation: {{ promotion.from_designation }}</h5>\r\n                      <h5 class=\"card-title\">Transfer Date: {{ promotion.transfer_date }}</h5>\r\n                      <h5 class=\"card-title\">Join Date: {{ promotion.join_date }}</h5>\r\n                      <h5 class=\"card-title\">Transfer Letter: {{ promotion.transfer_letter }}</h5>\r\n                      <h5 class=\"card-title\">To Department: {{ promotion.to_department }}</h5>\r\n                      <h5 class=\"card-title\">From Department: {{ promotion.from_department }}</h5>\r\n                    </div>\r\n                  </div>\r\n  \r\n                </div>\r\n              </div>\r\n            </div>\r\n  \r\n  \r\n            <!-- End Transter Details Modal -->\r\n  \r\n  \r\n          </div>\r\n  \r\n          <hr>\r\n  \r\n          <div class=\"row\">\r\n            <div class=\"col-md-12\">\r\n  \r\n              <table class=\"table text-center\" id=\"datatable\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>ID</th>\r\n                    <th>Employee</th>\r\n                    <th>Promotion Ref Number</th>\r\n                    <th>Promoted Designation</th>\r\n                    <th>Promotion Date</th>\r\n                    <th>Description</th>\r\n                    <th>Status</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <!-- \"employee_id\": 1,\r\n      \"employee_name\": \"Shakiur\",\r\n      \"id\": 3,\r\n      \"name\": null,\r\n      \"promotion_ref_number\": \"sdf\",\r\n      \"promoted_designation\": 22,\r\n      \"promotion_date\": \"2023-03-28\",\r\n      \"description\": \"sfd\",\r\n      \"status\": 1,\r\n      \"created_at\": \"2023-10-30T08:19:36.000000Z\",\r\n      \"updated_at\": \"2023-10-30T08:23:44.000000Z\" -->\r\n                  <tr v-for=\"(item, index) in promotionList1.data\" :key=\"index\">\r\n                    <td>{{ item.id }}</td>\r\n                    <td>{{ item.employee_name }}</td>\r\n                    <td>{{ item.promotion_ref_number }}</td>\r\n                    <td>{{ item.promoted_designation }}</td>\r\n                    <td>{{ item.promotion_date }}</td>\r\n                    <td>{{ item.description }}</td>\r\n                    <td>\r\n                      <p v-if=\"item.status == 1\" class=\"text-warning font-weight-bold\"> Verified</p>\r\n                      <p v-else class=\"text-success font-weight-bold\">Pending</p>\r\n                    </td>\r\n                    <td style=\"width: 185px;\">\r\n                      <button v-if=\"item.status === 1\" @click=\"viewDetails(item.id)\" class=\"btn btn-outline-primary mr-2\"\r\n                        style=\"border: 3px solid;font-weight: bolder; width: 124px;\" data-toggle=\"modal\"\r\n                        data-target=\"#exampleModal2\">\r\n                        View Details\r\n                      </button>\r\n                      <div v-else>\r\n                        <button @click=\"editbutton(item.id)\" class=\"btn btn-primary mr-2\" data-toggle=\"modal\"\r\n                          data-target=\"#exampleModal\">\r\n                          Edit\r\n                        </button>\r\n                        <button @click=\"deletebutton(item.id)\" class=\"btn btn-danger mr-2\">\r\n                          Delete\r\n                        </button>\r\n                      </div>\r\n  \r\n  \r\n                    </td>\r\n  \r\n                  </tr>\r\n  \r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Modal Start -->\r\n  \r\n  \r\n  \r\n    </div>\r\n  </template>\r\n    \r\n  <script>\r\n  \r\n  import \"jquery/dist/jquery.min.js\";\r\n  import \"bootstrap/dist/css/bootstrap.min.css\";\r\n  import \"datatables.net-dt/js/dataTables.dataTables\";\r\n  import \"datatables.net-dt/css/jquery.dataTables.min.css\";\r\n  import $ from \"jquery\";\r\n  \r\n  \r\n  \r\n  \r\n  // DataTable.use(DataTablesCore);\r\n  export default {\r\n    components: {\r\n      // DataTable,\r\n  \r\n  \r\n    },\r\n  \r\n  \r\n    data() {\r\n      cons