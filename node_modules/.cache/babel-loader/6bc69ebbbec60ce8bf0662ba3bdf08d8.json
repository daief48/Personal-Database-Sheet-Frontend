l + 'addTransferRecord', this.tansfer).then(res => {\n        console.log(res.data);\n        this.getProfileInfo();\n        this.$router.push({\n          path: '/transfer'\n        });\n      });\n    }\n\n  },\n\n  created() {\n    this.getTransferList();\n    this.getDepartmentList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser();\n    this.getOffice();\n  },\n\n  mounted() {},\n\n  watch: {}\n};","map":{"version":3,"mappings":"AAoNA,OAAOA,SAAP,MAAsB,qBAAtB;AACA,OAAOC,cAAP,MAA2B,gBAA3B,C,CACA;;AACA,OAAO,2BAAP;AACA,OAAO,sCAAP;AACA,OAAO,4CAAP;AACA,OAAO,iDAAP;AACA,OAAOC,CAAP,MAAc,QAAd;AACAF,SAAS,CAACG,GAAV,CAAcF,cAAd;AACA,eAAe;EACbG,UAAU,EAAE;IACVJ;EADU,CADC;;EAMbK,IAAI,GAAG;IACL,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX,CAApB;IACA,MAAMC,OAAM,GAAI,CACd;MAAEN,IAAI,EAAE;IAAR,CADc,EAEd;MAAEA,IAAI,EAAE;IAAR,CAFc,EAGd;MAAEA,IAAI,EAAE;IAAR,CAHc,EAId;MAAEA,IAAI,EAAE;IAAR,CAJc,EAKd;MAAEA,IAAI,EAAE;IAAR,CALc,EAMd;MAAEA,IAAI,EAAE;IAAR,CANc,EAOd;MAAEA,IAAI,EAAE;IAAR,CAPc,EAQd;MAAEA,IAAI,EAAE;IAAR,CARc,EASd;MAAEA,IAAI,EAAE;IAAR,CATc,EAUd;MAAEA,IAAI,EAAE;IAAR,CAVc,EAWd;MAAEA,IAAI,EAAE;IAAR,CAXc,EAYd;MAAEA,IAAI,EAAE;IAAR,CAZc,EAad;MACEO,MAAM,CAACP,IAAD,EAAOQ,IAAP,EAAaC,GAAb,EAAkB;QACtB,OAAQ,yDAAwDA,GAAG,CAACC,EAAG,MAAKD,GAAG,CAACC,EAAG,iBAAnF;MACF;;IAHF,CAbc,CAAhB;IAmBA,OAAO;MACLC,WAAW,EAAEC,OAAO,CAACC,GAAR,CAAYC,eADpB;MAELb,WAFK;MAGLc,IAAI,EAAE,IAHD;MAILC,WAAW,EAAE,IAJR;MAKLC,YAAY,EAAE,IALT;MAMLC,YAAY,EAAE,IANT;MAOLC,MAAM,EAAE,IAPH;MAQLC,GAAG,EAAE,KAAKC,UAAL,GAAkB,YARlB;MASLC,SAAS,EAAE;QACTC,IAAI,EAAE,EADG;QAETC,YAAY,EAAE,EAFL;QAGTC,KAAK,EAAE,EAHE;QAITC,WAAW,EAAE,EAJJ;QAKTC,cAAc,EAAE,EALP;QAMTC,KAAK,EAAE;MANE,CATN;MAiBLtB,OAjBK;MAkBLuB,YAAY,EAAE,EAlBT;MAmBLC,OAAO,EAAE;QACPC,WAAW,EAAE,IADN;QAEPC,aAAa,EAAE,IAFR;QAGPC,cAAc,EAAE,IAHT;QAIPC,qBAAqB,EAAE,IAJhB;QAKPC,SAAS,EAAE,IALJ;QAMPC,WAAW,EAAE,IANN;QAOPC,aAAa,EAAE,IAPR;QAQPC,eAAe,EAAE,IARV;QASPC,cAAc,EAAE,IATT;QAUPC,gBAAgB,EAAE,IAVX;QAWPC,aAAa,EAAE,IAXR;QAYPC,SAAS,EA