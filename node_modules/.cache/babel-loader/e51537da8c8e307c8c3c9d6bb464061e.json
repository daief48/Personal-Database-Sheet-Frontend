d_url + 'deleteTransferType/' + id).then(res => {\n        console.log('Delete Successfully', res.message);\n        this.getTransferTypeList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(\"Delete Successfully\");\n      }).catch(error => {\n        console.error(\"An error occurred while updating the transfer record:\", error);\n        this.$toast.error(\"Record cannot be deleted. Associated data exists.\");\n      });\n    },\n    EdittrainingType() {\n      try {\n        this.axios.post(this.backend_url + `updateTransferType/${this.edittrainingType.id}`, this.edittrainingType).then(res => {\n          console.log(res);\n          this.getTransferTypeList();\n          $(\"#exampleModal1\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Update Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while updating the transfer record:\", error);\n          this.$toast.error(`Failed to update transfer record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to update transfer record.`);\n      }\n    },\n    getTransferTypeList() {\n      this.axios.get(this.backend_url + 'transferType').then(res => {\n        console.log(res.data.list);\n        this.transferTypeList = res.data.list;\n        setTimeout(function () {\n          $(\"#datatable\").DataTable({\n            \"processing\": true\n          });\n        }, 100);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    editbutton(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificTransferType/' + id).then(response => {\n        this.transferRecordById = response.data.data;\n        //   trainingType: {\n        //         title: \"\",\n        //         status: \"\"\n        //     }\n        this.edittrainingType.id = id;\n        this.edittrainingType.title = this.transferRecordById.title;\n        this.edittrainingType.status = this.transferRecordById.status;\n        console.log('edit ', this.edittrainingType);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list;\n        // console.log(this.office);\n      });\n    },\n\n    activestatus(id) {\n      this.axios.patch(this.backend_url + `activeTransferType/${id}`).then(response => {\n        console.log(response);\n        this.getTransferTypeList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.success(`Active status Successfully!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    inactivestatus(id) {\n      this.axios.patch(this.backend_url + `inactiveTransferTypeRecord/${id}`).then(response => {\n        console.log(response);\n        this.getTransferTypeList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(`Status Cancel!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    savetrainingType() {\n      try {\n        // this.trainingType.employee_id = this.storageData.id;\n        this.edit = false;\n        this.axios.post(this.backend_url + 'addTransferType', this.trainingType).then(res => {\n          console.log(res);\n          this.getTransferTypeList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Add Data Successfully!`);\n          this.$refs.form.resetForm();\n        }).catch(error => {\n          console.error(\"An error occurred while saving the transfer record:\", error);\n          this.$toast.error(`Failed to save transfer record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to save transfer record.`);\n      }\n    }\n  },\n  created() {\n    this.getTransferTypeList();\n    this.getOffice();\n  },\n  watch: {}\n};","map":{"version":3,"mappings":"AAqSA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAO,2BAA2B;AAClC,OAAO,sCAAsC;AAC7C,OAAO,4CAA4C;AACnD,OAAO,iDAAiD;AACxD,OAAOC,OAAO,QAAQ;AACtB,SAASC,IAAI,EAAEC,KAAI,QAAU,cAAc;AAC3C,OAAO,KAAKC,GAAE,MAAO,KAAK;;AAE1B;AACA,eAAe;EACXC,UAAU,EAAE;IACR;IACAH,IAAI;IAAEC;EACV,CAAC;EAEDG,IAAIA,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;IAE5D,MAAMC,MAAK,GAAIR,GAAG,CAACS,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC;MAC9BC,KAAK,EAAEX,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,+BAA+B,CAAC;MAC7DC,MAAM,EAAEd,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,oBAAoB;IACtD,CAAC,CAAC;IAEF,MAAME,UAAS,GAAIf,GAAG,CAACS,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC;MAClCM,SAAS,EAAEhB,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,+BAA+B,CAAC;MACjEI,UAAU,EAAEjB,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,oBAAoB;IAC1D,CAAC,CAAC;IACF,IAAIK,gBAAe,GAAItB,GAAG,CAACY,MAAM,CAAC;;IAElC;IACA,MAAMW,qBAAoB,GAAIA,MAAM;MAChCD,gBAAgB,CAACE,KAAI,GAAI,IAAI,EAAE;IACnC,CAAC;;IAED,OAAO;MACHZ,MAAM;MACNU,gBAAgB;MAACC,qBAAqB;MACtCJ,UAAU;MACVM,WAAW,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAe;MACxCrB,WAAW;MACXsB,IAAI,EAAE,EAAE;MACRC,aAAa,EAAE,EAAE;MACjBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,EAAE;MAChBC,MAAM,EAAE,EAAE;MACVC,GAAG,EAAE,IAAI,CAACC,UAAS,GAAI,YAAY;MACnCC,SAAS,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,cAAc,EAAE,EAAE;QAClBC,KAAK,EAAE;MACX,CAAC;MACDC,kBAAkB,EAAE,EAAE;MACtBC,gBAAgB,EAAE,EAAE;MACpBC,YAAY,EAAE;QACV9B,KAAK,EAAE,EAAE;QACTG,MAAM,EAAE;MACZ,CAAC;MACD4B,gBAAgB,EAAE;QACd/B,KAAK,EAAE,EAAE;QACTG,MAAM,EAAE;MACZ;IAEJ,CAAC;EACL,CAAC;EAED6B,QAAQ,EAAE,CAAC,CAAC;EAEZC,OAAO,EAAE;IACLC,YAAYA,CAACC,EAAE,EAAE;MACb,IAAI,CAACC,KAAI,CACJC,MAAM,CAAC,IAAI,CAAC3B,WAAU,GAAI,qBAAoB,GAAIyB,EAAE,EACpDG,IAAI,CAAEC,GAAG,IAAK;QACXC,OAAO,CAACC,GAAG,CAAC,qBAAqB,EAAEF,GAAG,CAACG,OAAO,CAAC;QAE/C,IAAI,CAACC,mBAAmB,CAAC,CAAC;QAC1BzD,CAAC,CAAC,YAAY,CAAC,CAAC0D,SAAS,CAAC,CAAC,CA