{"ast":null,"code":"import \"jquery/dist/jquery.min.js\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"datatables.net-dt/js/dataTables.dataTables\";\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\nimport $ from \"jquery\";\nimport { Form, Field } from 'vee-validate';\nimport * as Yup from \"yup\";\n\n// DataTable.use(DataTablesCore);\nexport default {\n  components: {\n    // DataTable,\n    Form,\n    Field\n  },\n  data() {\n    const storageData = JSON.parse(localStorage.getItem('user'));\n    // training: {\n    //         employee_id: \"\",\n    //         training_center_name: \"\",\n    //         training_name: \"\",\n    //         training_score: \"\",\n    //         training_feedback: \"\",\n    //         training_strt_date: \"\",\n    //         training_end_date: \"\",\n    //     }\n    const schema = Yup.object().shape({\n      training_center_name: Yup.string().required(\"Training Center Name is required\"),\n      training_name: Yup.string().required(\"Training Name is required\"),\n      description: Yup.string().required(\"Description is required\"),\n      // Corrected error message\n      training_score: Yup.string().required(\"Training Score is required\"),\n      training_feedback: Yup.string().required(\"Training Feedback is required\"),\n      training_start_date: Yup.date().required(\"Training Start Date is required\").typeError(\"Invalid date format\"),\n      training_strt_date: Yup.date().required(\"Training End Date is required\").typeError(\"Invalid date format\"),\n      training_end_date: Yup.date().required(\"Training End Date is required\").typeError(\"Invalid date format\")\n    });\n    return {\n      schema,\n      backend_url: process.env.VUE_APP_API_URL,\n      storageData,\n      user: \"\",\n      departments: \"\",\n      designations: \"\",\n      TrainingType: \"\",\n      office: \"\",\n      edit: false,\n      url: this.backendUrl + \"/api/users\",\n      basicInfo: {\n        name: '',\n        mobileNumber: '',\n        email: '',\n        designation: '',\n        designation_id: '',\n        image: ''\n      },\n      trainingRecordById: [],\n      trainingList1: [],\n      //         <!-- \"employee_id\": 1,\n      //   \"employee_name\": \"Shakiur\",\n      //   \"id\": 2,\n      //   \"training_center_name\": \"322\",\n      //   \"training_name\": \"322\",\n      //   \"training_score\": 322,\n      //   \"training_feedback\": \"322\",\n      //   \"training_strt_date\": \"2023-10-29\",\n      //   \"training_end_date\": \"2023-10-29\",\n      //   \"description\": \"322\",\n      //   \"status\": 0,\n      //   \"created_at\": \"2023-10-29T06:30:12.000000Z\",\n      //   \"updated_at\": \"2023-10-29T06:42:12.000000Z\" -->\n      training: {\n        employee_id: \"\",\n        training_center_name: \"\",\n        training_name: \"\",\n        description: \"\",\n        training_score: \"\",\n        training_feedback: \"\",\n        training_strt_date: \"\",\n        training_end_date: \"\"\n      }\n    };\n  },\n  computed: {},\n  methods: {\n    clear() {\n      this.training.training_center_name = \"\";\n      this.training.training_name = \"\";\n      this.training.training_score = \"\";\n      this.training.training_feedback = \"\";\n      this.training.training_strt_date = \"\";\n      this.training.training_end_date = \"\";\n      this.training.description = \"\";\n    },\n    editbutton(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificUserTraining/' + id).then(response => {\n        this.trainingRecordById = response.data.data;\n        console.log(this.trainingRecordById);\n        this.training.id = id;\n        this.training.employee_id = this.trainingRecordById.employee_id;\n        this.training.training_center_name = this.trainingRecordById.training_center_name;\n        this.training.training_name = this.trainingRecordById.training_name;\n        this.training.training_score = this.trainingRecordById.training_score;\n        this.training.training_feedback = this.trainingRecordById.training_feedback;\n        this.training.training_strt_date = this.trainingRecordById.training_strt_date;\n        this.training.training_end_date = this.trainingRecordById.training_end_date;\n        this.training.description = this.trainingRecordById.description;\n        this.edit = true;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    deletebutton(id) {\n      this.axios.delete(this.backend_url + 'deleteTrainingRecord/' + id).then(res => {\n        console.log('Delete Successfully', res);\n        this.getTrainingList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error('Delete Successfully');\n      });\n    },\n    viewDetails(id) {\n      console.log(id);\n      // <!-- training: {\n      //     employee_id: \"\",\n      //     training_center_name: \"\",\n      //     training_name: \"\",\n      //     training_score: \"\",\n      //     training_feedback: \"\",\n      //     training_strt_date: \"\",\n      //     training_end_date: \"\",\n      // } -->\n      this.axios.get(this.backend_url + 'specificUserTraining/' + id).then(response => {\n        this.trainingRecordById = response.data.data;\n        console.log(this.trainingRecordById);\n        this.training.id = id;\n        this.training.employee_id = this.trainingRecordById.employee_id;\n        this.training.training_center_name = this.trainingRecordById.training_center_name;\n        this.training.training_name = this.trainingRecordById.training_name;\n        this.training.training_score = this.trainingRecordById.training_score;\n        this.training.training_feedback = this.trainingRecordById.training_feedback;\n        this.training.training_strt_date = this.trainingRecordById.training_strt_date;\n        this.training.training_end_date = this.trainingRecordById.training_end_date;\n        this.training.description = this.trainingRecordById.description;\n        this.edit = false;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getTrainingList() {\n      this.axios.get(this.backend_url + 'getTrainingList=' + this.storageData.id).then(res => {\n        console.log(res.data.list);\n        this.trainingList1 = res.data.list;\n        setTimeout(function () {\n          $(\"#datatable\").DataTable({\n            \"processing\": true\n          });\n        }, 100);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getDepartmentList() {\n      this.axios.get(this.backend_url + 'getDepartment').then(res => {\n        // console.log(res.data.data);\n        this.departments = res.data.data;\n      });\n    },\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data;\n        // console.log(this.designations);\n      });\n    },\n\n    getTrainingtype() {\n      this.axios.get(this.backend_url + 'TrainingType').then(res => {\n        this.TrainingType = res.data.list;\n        // console.log('fddsfsdf');\n        // console.log(this.TrainingType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id;\n        // console.log(this.user);\n        this.training.employee_id = this.user;\n        // console.log(this.storageData.id);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list;\n        // console.log(this.office);\n      });\n    },\n\n    saveTraining() {\n      // this.training.employee_id = this.storageData.id;\n      this.edit = false;\n      this.axios.post(this.backend_url + 'addTrainingRecord', this.training).then(res => {\n        console.log(res);\n        $(\"#exampleModal\").modal('hide');\n        this.getTrainingList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.success(`Data Added Successfully!!`);\n      }).catch(error => {\n        console.error(\"An error occurred while updating the training record:\", error);\n        this.$toast.error(error);\n      });\n    },\n    EditTraining() {\n      try {\n        this.axios.post(this.backend_url + `updateTrainingRecord/${this.training.id}`, this.training).then(res => {\n          console.log(res);\n          $(\"#exampleModal\").modal('hide');\n          this.getTrainingList();\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Update Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while updating the training record:\", error);\n          this.$toast.error(`Failed to update training record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to update training record.`);\n      }\n    }\n  },\n  created() {\n    this.getTrainingList();\n    this.getDepartmentList();\n    this.getDesignationList();\n    this.getTrainingtype();\n    this.getUser();\n    this.getOffice();\n  },\n  mounted() {},\n  watch: {}\n};","map":{"version":3,"mappings":"AA4QA,OAAO,2BAA2B;AAClC,OAAO,sCAAsC;AAC7C,OAAO,4CAA4C;AACnD,OAAO,iDAAiD;AACxD,OAAOA,OAAO,QAAQ;AACtB,SAASC,IAAI,EAAEC,KAAI,QAAS,cAAc;AAC1C,OAAO,KAAKC,GAAE,MAAO,KAAK;;AAE1B;AACA,eAAe;EACXC,UAAU,EAAE;IACR;IACAH,IAAI;IAAEC;EAEV,CAAC;EAGDG,IAAIA,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;IAC5D;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA,MAAMC,MAAK,GAAIR,GAAG,CAACS,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC;MAC9BC,oBAAoB,EAAEX,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,kCAAkC,CAAC;MAC/EC,aAAa,EAAEd,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,2BAA2B,CAAC;MACjEE,WAAW,EAAEf,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,yBAAyB,CAAC;MAAE;MAC/DG,cAAc,EAAEhB,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,4BAA4B,CAAC;MACnEI,iBAAiB,EA