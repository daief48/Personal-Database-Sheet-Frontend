      console.log(this.designations);\n      });\n    },\n\n    saveTranfer() {\n      let formData = new FormData();\n      formData.append('user_id', this.storageData.id);\n      this.axios.post(this.backend_url + 'saveTranfer', formData).then(res => {\n        console.log(res.data);\n        this.getProfileInfo();\n      });\n    }\n\n  },\n\n  created() {\n    this.getTransferList(); // this.getDepartmentList();\n    // this.getDesignationList();\n  },\n\n  watch: {}\n};","map":{"version":3,"mappings":"AA0CA,OAAOA,SAAP,MAAsB,qBAAtB;AACA,OAAOC,cAAP,MAA2B,gBAA3B;AACAD,SAAS,CAACE,GAAV,CAAcD,cAAd;AACA,eAAe;EACbE,UAAU,EAAE;IACVH;EADU,CADC;;EAKbI,IAAI,GAAG;IACL,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX,CAApB;IACA,MAAMC,OAAM,GAAI,CAChB;MAAEN,IAAI,EAAE;IAAR,CADgB,EAEd;MAAEA,IAAI,EAAE;IAAR,CAFc,EAGd;MAAEA,IAAI,EAAE;IAAR,CAHc,EAId;MAAEA,IAAI,EAAE;IAAR,CAJc,EAKd;MAAEA,IAAI,EAAE;IAAR,CALc,EAMd;MAAEA,IAAI,EAAE;IAAR,CANc,EAOd;MAAEA,IAAI,EAAE;IAAR,CAPc,EAQd;MAAEA,IAAI,EAAE;IAAR,CARc,EASd;MAAEA,IAAI,EAAE;IAAR,CATc,EAUd;MAAEA,IAAI,EAAE;IAAR,CAVc,EAWd;MACEO,KAAK,EAAE,SADT;MAEEC,SAAS,EAAE,KAFb;MAGEC,UAAU,EAAE,KAHd;;MAIEC,MAAM,CAACV,IAAD,EAAOW,IAAP,EAAaC,GAAb,EAAkB;QACtB,OAAO;+DAC8CA,GAAG,CAACC,EAAG;gEACND,GAAG,CAACC,EAAG;WAF7D;MAID;;IATH,CAXc,CAAhB;IAuBA,OAAO;MACLC,WAAW,EAAEC,OAAO,CAACC,GAAR,CAAYC,eADpB;MAELhB,WAFK;MAGLiB,GAAG,EAAE,KAAKC,UAAL,GAAkB,YAHlB;MAILC,SAAS,EAAE;QACTC,IAAI,EAAE,EADG;QAETC,YAAY,EAAE,EAFL;QAGTC,KAAK,EAAE,EAHE;QAITC,WAAW,EAAE,EAJJ;QAKTC,cAAc,EAAE,EALP;QAMTC,KAAK,EAAE;MANE,CAJN;MAYLpB,OAZK;MAaLqB,YAAY,EAAE;IAbT,CAAP;EAgBD,CA9CY;;EAgDbC,QAAQ,EAAE,EAhDG;EAkDbC,OAAO,EAAE;IAEPC,eAAe,GAAG;MAChB,KAAKC,KAAL,CACGC,GADH,CACO,KAAKlB,WAAL,GAAmB,0BAAnB,GAAgD,KAAKb,WAAL,CAAiBY,EADxE,EAEGoB,IAFH,CAESC,GAAD,IAAS;QACbC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAAClC,IAAJ,CAASA,IAArB;QACA,KAAK2B,YAAL,GAAoBO,GAAG,CAAClC,IAAJ,CAASA,IAA7B;MACD,CALH,EAMGqC,KANH,CAMUC,CAAD,IAAO;QACZH,OAAO,CAACC,GAAR,CAAYE,CAAZ;MACD,CARH;IASD,CAZM;;IAcPC,iBAAiB,GAAG;MAClB,KAAKR,KAAL,