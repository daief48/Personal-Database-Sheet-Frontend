  <div class=\"card-body\">\r\n           <div class=\"row\">\r\n            <div class=\"col-md-12\">\r\n                <table class=\"table table-hover\" id=\"datatable\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th scope=\"col\">SL</th>\r\n                            <th scope=\"col\">Name</th>\r\n                            <th scope=\"col\">Email</th>\r\n                            <th scope=\"col\">Subject</th>\r\n                            <th scope=\"col\">Action</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr v-for=\"(message,index) in messages\" :key=\"message.id\">\r\n                            <td scope=\"row\">{{index+1}}</td>\r\n                            <td>{{message.name}}</td>\r\n                            <td>{{message.email}}</td>\r\n                            <td>{{message.subject}}</td>\r\n                            <td>\r\n                            <button class=\"btn btn-info mx-2 btn-sm\" title=\"Message Details\"\r\n                            @click=\"onShow(message)\">\r\n                            <font-awesome-icon icon=\"fa-solid  fa-info-circle\" />\r\n                            </button>\r\n                            |\r\n                            <button class=\"btn btn-info mx-2 btn-sm\" title=\"Reply\" \r\n                            @click=\"onReply(message)\">\r\n                            <font-awesome-icon icon=\"fa-solid fa-envelope\" />\r\n                            </button>\r\n                        </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n           </div> \r\n        <loader v-if=\"spinnerOn\"></loader>\r\n        </div>\r\n    </div>\r\n        <!-- Modal For Schedule -->\r\n        <div class=\"modal fade\" id=\"exampleModalCenter\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exa