{"ast":null,"code":"import { Form, Field, ErrorMessage } from \"vee-validate\";\nimport * as yup from \"yup\";\nimport { useStore } from \"vuex\"; //import VPagination from \"@hennge/vue3-pagination\";\n\nimport \"@hennge/vue3-pagination/dist/vue3-pagination.css\";\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage // VPagination,\n\n  },\n\n  data() {\n    const store = useStore();\n    const validationCreate = yup.object().shape({\n      importantLinkTitle: yup.string().required(\"Important Link Title is required!\").min(3, \"Must be at least 3 characters!\").max(20, \"Must be maximum 20 characters!\")\n    });\n    return {\n      backend_url: process.env.VUE_APP_API_URL,\n      url: this.backendUrl + \"/api/links\",\n      store,\n      validationCreate: validationCreate,\n      userForm: validationCreate\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,SAASA,IAAT,EAAeC,KAAf,EAAsBC,YAAtB,QAA0C,cAA1C;AACA,OAAO,KAAKC,GAAZ,MAAqB,KAArB;AACA,SAAQC,QAAR,QAAuB,MAAvB,C,CACA;;AACA,OAAO,kDAAP;AAEA,eAAe;EACXC,UAAU,EAAE;IACRL,IADQ;IAERC,KAFQ;IAGRC,YAHQ,CAIT;;EAJS,CADD;;EAOXI,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIH,QAAQ,EAAtB;IACA,MAAMI,gBAAe,GAAIL,GAAG,CAACM,MAAJ,GAAaC,KAAb,CAAmB;MACxCC,kBAAkB,EAAER,GAAE,CACjBS,MADe,GAEfC,QAFe,CAEN,mCAFM,EAGfC,GAHe,CAGX,CAHW,EAGR,gCAHQ,EAIfC,GAJe,CAIX,EAJW,EAIP,gCAJO;IADoB,CAAnB,CAAzB;IAQA,OAAO;MACHC,WAAW,EAAEC,OAAO,CAACC,GAAR,CAAYC,eADtB;MAEHC,GAAG,EAAE,KAAKC,UAAL,GAAkB,YAFpB;MAGHd,KAHG;MAIHC,gBAAgB,EAAEA,gBAJf;MAKHc,QAAQ,EAAEd;IALP,CAAP;EAQH;;AAzBU,CAAf","names":["Form","Field","ErrorMessage","yup","useStore","components","data","store","validationCreate","object","shape","importantLinkTitle","string","required","min","max","backend_url","process","env","VUE_APP_API_URL","url","backendUrl","userForm"],"sourceRoot":"","sources":["/var/www/html/PMS_PROJECT/pds/src/components/Page/Page.vue"],"sourcesContent":["<template>\n     <div class=\"card card-default\">\n        <div class=\"card-header\">\n            <h3 class=\"card-title\"><strong> Important Link   {{backend_url}}{{formAction}}</strong></h3>\n        </div>\n\n        <div class=\"card-body\">\n            <div class=\"row\">\n                <div class=\"col-md-3 form-bg\">\n                    <Form @submit=\"handleUser\" :validation-schema=\"userForm\" ref=\"form\">\n                        <div>\n                        <div class=\"form-group\">\n                            <label for=\"title\">Title*</label>\n                            <Field name=\"importantLinkTitle\" type=\"text\" v-model=\"importantLinkTitle\" class=\"form-control\" autocomplete=\"off\"/>\n                            <ErrorMessage name=\"importantLinkTitle\" v-model=\"importantLinkTitle\" class=\"error-feedback\" />\n                        </div>\n\n                        </div>\n                    </Form>\n                </div>\n            </div>\n        </div>\n\n     </div>\n</template>\n\n<script>\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nimport * as yup from \"yup\";\nimport {useStore} from \"vuex\"\n//import VPagination from \"@hennge/vue3-pagination\";\nimport \"@hennge/vue3-pagination/dist/vue3-pagination.css\";\n\nexport default {\n    components: {\n        Form,\n        Field,\n        ErrorMessage,\n       // VPagination,\n    },\n    data() {\n        const store = useStore();\n        const validationCreate = yup.object().shape({\n            importantLinkTitle: yup\n                .string()\n                .required(\"Important Link Title is required!\")\n                .min(3, \"Must be at least 3 characters!\")\n                .max(20, \"Must be maximum 20 characters!\"),\n   \n        });\n        return {\n            backend_url: process.env.VUE_APP_API_URL,\n            url: this.backendUrl + \"/api/links\",\n            store,\n            validationCreate: validationCreate,\n            userForm: validationCreate,\n            \n        }\n    },\n    \n\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}