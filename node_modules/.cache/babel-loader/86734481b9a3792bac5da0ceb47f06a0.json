{"ast":null,"code":"import \"jquery/dist/jquery.min.js\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"datatables.net-dt/js/dataTables.dataTables\";\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\nimport $ from \"jquery\";\nimport { Form, Field } from 'vee-validate';\nimport * as Yup from \"yup\";\n\n// DataTable.use(DataTablesCore);\nexport default {\n  components: {\n    // DataTable\n    Form,\n    Field\n  },\n  data() {\n    const storageData = JSON.parse(localStorage.getItem('user'));\n    const schema = Yup.object().shape({\n      designation_name: Yup.string().required(\"Designation Name is required\"),\n      status: Yup.string().required(\"Status is required\")\n    });\n    return {\n      schema,\n      backend_url: process.env.VUE_APP_API_URL,\n      storageData,\n      user: \"\",\n      designations: \"\",\n      transferType: \"\",\n      office: \"\",\n      edit: false,\n      url: this.backendUrl + \"/api/users\",\n      basicInfo: {\n        name: '',\n        mobileNumber: '',\n        email: '',\n        designation: '',\n        designation_id: '',\n        image: ''\n      },\n      transferRecordById: [],\n      transferList: [],\n      designation: {\n        designation_name: \"\",\n        status: \"\"\n      }\n    };\n  },\n  computed: {},\n  methods: {\n    clear() {\n      this.designation.designation_name = \"\";\n      this.designation.status = \"\";\n    },\n    getTransferList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt?user_id=' + this.storageData.id).then(res => {\n        console.log(res.data.data);\n        this.transferList = res.data.data;\n        setTimeout(function () {\n          $(\"#datatable\").DataTable({\n            \"processing\": true\n          });\n        }, 100);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    deletebutton(id) {\n      this.axios.delete(this.backend_url + 'deleteTransferRecord/' + id).then(res => {\n        console.log('Delete Successfully', res);\n        this.getTransferList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(`Delete Successfully!!`);\n      });\n    },\n    viewDetails(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificUserTransferRecord/' + id).then(response => {\n        console.log(response);\n        this.transferRecordById = response.data.data;\n        console.log(this.transferRecordById.transfer_type);\n        this.designation.id = id;\n        this.designation.employee_name = this.transferRecordById.employee_name;\n        this.designation.employee_id = this.transferRecordById.employee_id;\n        this.designation.transfer_type = this.transferRecordById.transfer_type;\n        this.designation.transfer_order = this.transferRecordById.transfer_order;\n        this.designation.transfer_order_number = this.transferRecordById.transfer_order_number;\n        this.designation.to_office = this.transferRecordById.to_office;\n        this.designation.from_office = this.transferRecordById.from_office;\n        this.designation.to_designation = this.transferRecordById.to_designation;\n        this.designation.from_designation = this.transferRecordById.from_designation;\n        this.designation.transfer_date = this.transferRecordById.transfer_date;\n        this.designation.join_date = this.transferRecordById.join_date;\n        this.designation.transfer_letter = this.transferRecordById.transfer_letter;\n        this.designation.to_designation = this.transferRecordById.to_designation;\n        this.designation.from_designation = this.transferRecordById.from_designation;\n        this.edit = false;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getdesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        // console.log(res.data.data);\n        this.designations = res.data.data;\n      });\n    },\n    getTransfertype() {\n      this.axios.get(this.backend_url + 'transferType').then(res => {\n        this.transferType = res.data.list;\n        // console.log('fddsfsdf');\n        // console.log(this.transferType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id;\n        // console.log(this.user);\n        // this.designation.employee_id = this.user\n        // console.log(this.storageData.id);\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data;\n        // console.log(this.designations);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list;\n        // console.log(this.office);\n      });\n    },\n\n    activestatus(id) {\n      this.axios.patch(this.backend_url + `activeDesignationMgtRecord/${id}`).then(response => {\n        console.log(response);\n        this.getTransferList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.success(`Active status Successfully!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    inactivestatus(id) {\n      this.axios.patch(this.backend_url + `inactiveDesignationMgtRecord/${id}`).then(response => {\n        console.log(response);\n        this.getTransferList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(`Status Cancel!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    saveTranfer() {\n      try {\n        // this.designation.employee_id = this.storageData.id;\n        this.edit = false;\n        this.axios.post(this.backend_url + 'addDesignationMgt', this.designation).then(res => {\n          console.log(res);\n          this.getTransferList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Add Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while saving the transfer record:\", error);\n          this.$toast.error(`Failed to save transfer record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to save transfer record.`);\n      }\n    }\n  },\n  created() {\n    this.getTransferList();\n    this.getdesignationList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser();\n    this.getOffice();\n  },\n  watch: {}\n};","map":{"version":3,"mappings":"AAoNA,OAAO,2BAA2B;AAClC,OAAO,sCAAsC;AAC7C,OAAO,4CAA4C;AACnD,OAAO,iDAAiD;AACxD,OAAOA,OAAO,QAAQ;AACtB,SAASC,IAAI,EAAEC,KAAI,QAAS,cAAc;AAC1C,OAAO,KAAKC,GAAE,MAAO,KAAK;;AAE1B;AACA,eAAe;EACXC,UAAU,EAAE;IACR;IACAH,IAAI;IAAEC;EACV,CAAC;EAEDG,IAAIA,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;IAE5D,MAAMC,MAAK,GAAIR,GAAG,CAACS,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC;MAC9BC,gBAAgB,EAAEX,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,8BAA8B,CAAC;MACvEC,MAAM,EAAEd,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,oBAAoB;IAEtD,CAAC,CAAC;IAGF,OAAO;MACHL,MAAM;MACNO,WAAW,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAe;MACxCf,WAAW;MACXgB,IAAI,EAAE,EAAE;MACRC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,EAAE;MAChBC,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE,KAAK;MACXC,GAAG,EAAE,IAAI,CAACC,UAAS,GAAI,YAAY;MACnCC,SAAS,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,cAAc,EAAE,EAAE;QAClBC,KAAK,EAAE;MACX,CAAC;MACDC,kBAAkB,EAAE,EAAE;MACtBC,YAAY,EAAE,EAAE;MAChBJ,WAAW,EAAE;QACTnB,gBAAgB,EAAE,EAAE;QACpBG,MAAM,EAAE;MACZ;IAEJ,CAAC;EACL,CAAC;EAEDqB,QAAQ,EAAE,CAAC,CAAC;EAEZC,OAAO,EAAE;IACLC,KAAKA,GAAG;MACJ,IAAI,CAACP,WAAW,CAACnB,gBAAe,GAAI,EAAE;MACtC,IAAI,CAACmB,WAAW,CAAChB,MAAK,GAAI,EAAE;IAChC,CAAC;IACDwB,eAAeA,GAAG;MACd,IAAI,CAACC,KAAI,CACJC,GAAG,CAAC,IAAI,CAACzB,WAAU,GAAI,4