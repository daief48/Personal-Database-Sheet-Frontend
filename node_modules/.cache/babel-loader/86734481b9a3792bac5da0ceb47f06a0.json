hema=\"schema\"\r\n                                        v-slot=\"{ errors }\">\r\n                                        <div>\r\n                                            <div class=\"row\">\r\n                                                {{ designation }}\r\n                                                <div class=\"col-4 w-100\">\r\n\r\n                                                    <div data-v-b1d61d26=\"\" class=\"d-flex\"\r\n                                                        style=\"justify-content: space-around;\">\r\n                                                        <div class=\"form-group mr-2 w-50\">\r\n                                                            <label for=\"exampleFormControlSelect1\"> Designation Name</label>\r\n                                                            <Field type=\"text\" class=\"form-control\" id=\"exampleInputEmail1\"\r\n                                                                aria-describedby=\"emailHelp\" placeholder=\"designation Name\"\r\n                                                                v-model=\"designation.designation_name\" name=\"designation_name\"\r\n                                                                :class=\"{ 'is-invalid': errors.designation_name }\" />\r\n\r\n                                                            <div class=\"invalid-feedback\">{{ errors.designation_name }}</div>\r\n\r\n                                                        </div>\r\n\r\n\r\n\r\n                                                        <div class=\"form-group mr-2 w-50\">\r\n                                                            <label for=\"exampleFormControlSelect1\"> Status</label>\r\n                                                            <Field class=\"form-control\" as=\"select\"\r\n                                                                id=\"exampleFormControlSelect1\" v-model=\"designation.status\"\r\n                                                                name=\"status\" :class=\"{ 'is-invalid': errors.status }\">\r\n                                                                <option value=\"1\">Approved</option>\r\n                                                                <option value=\"0\">Pending</option>\r\n                                                            </Field>\r\n                                                            <div class=\"invalid-feedback\">{{ errors.status }}</div>\r\n\r\n                                                        </div>\r\n                                                    </div>\r\n                                                </div>\r\n\r\n\r\n                                            </div>\r\n                                        </div>\r\n                                        <div class=\"modal-footer d-flex justify-content-center\">\r\n                                            <button type=\"button\" class=\"btn btn-secondary\"\r\n                                                data-dismiss=\"modal\">Cancel</button>\r\n                                            <button type=\"submit\" class=\"btn btn-primary\" @click=\"EditTranfer()\"\r\n                                                v-if=\"edit\">Edit</button>\r\n                                            <button type=\"submit\" class=\"btn btn-primary\" @click=\"saveTranfer()\"\r\n                                                v-else>Add</button>\r\n                                        </div>\r\n                                    </Form>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <!--End Add and Edit Modal -->\r\n\r\n\r\n                    <!-- Transter Details Modal -->\r\n\r\n                    <div class=\"modal fade\" id=\"exampleModal2\" tabindex=\"-1\" role=\"dialog\"\r\n                        aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n                        <div class=\"modal-dialog\" role=\"document\">\r\n                            <div class=\"modal-content\" style=\"width: 121%; top: 164px;\">\r\n                                <div class=\"modal-header\">\r\n                                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                                        <span aria-hidden=\"true\">&times;</span>\r\n                                    </button>\r\n                                </div>\r\n                                <div class=\"modal-body card\">\r\n                                    <div class=\"card-body d-flex flex-column\">\r\n                                        <h5 class=\"modal-title text-center\" id=\"exampleModalLabel\" style=\"font-size: 25px;\">\r\n                                            Transfer Details\r\n                                        </h5>\r\n                                        <hr>\r\n                                        <h1 class=\"card-title \">Transfer Type: {{ designation.transfer_type }}</h1>\r\n                                        <h5 class=\"card-title\">Transfer Order: {{ designation.transfer_order }}</h5>\r\n                                        <h5 class=\"card-title\">Transfer Order Number: {{ designation.transfer_order_number }}\r\n                                        </h5>\r\n                                        <h5 class=\"card-title\">To Office: {{ designation.to_office }}</h5>\r\n                                        <h5 class=\"card-title\">From Office: {{ designation.from_office }}</h5>\r\n                                        <h5 class=\"card-title\">To Designation: {{ designation.to_designation }}</h5>\r\n                                        <h5 class=\"card-title\">From Designation: {{ designation.from_designation }}</h5>\r\n                                        <h5 class=\"card-title\">Transfer Date: {{ designation.transfer_date }}</h5>\r\n                                        <h5 class=\"card-title\">Join Date: {{ designation.join_date }}</h5>\r\n                                        <h5 class=\"card-title\">Transfer Letter: {{ designation.transfer_letter }}</h5>\r\n                                        <h5 class=\"card-title\">To designation: {{ designation.to_designation }}</h5>\r\n                                        <h5 class=\"card-title\">From designation: {{ designation.from_designation }}</h5>\r\n                                    </div>\r\n                                </div>\r\n\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n                    <!-- End Transter Details Modal -->\r\n\r\n\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-md-12\">\r\n                        <table class=\"table text-center\" id=\"datatable\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>ID</th>\r\n                                    <th>Designation Name</th>\r\n                                    <th>Create Date</th>\r\n                                    <th>Status</th>\r\n                                    <th>Action</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                <tr v-for=\"(item, index) in transferList\" :key=\"index\">\r\n                                    <td>{{ item.id }}</td>\r\n                                    <td>{{ item.designation_name }}</td>\r\n                                    <td>{{ item.created_at.slice(0, 10) }}</td>\r\n                                    <td>\r\n                                        <p v-if=\"item.status == 1\" class=\"text-success font-weight-bold\"> Verified</p>\r\n                                        <p v-else-if=\"item.status == 2\" class=\"text-danger font-weight-bold\"> Cancel</p>\r\n                                        <p v-else class=\"text-warning font-weight-bold\">Pending</p>\r\n                                    </td>\r\n                                    <td style=\"width: 185px;\">\r\n                                        <div v-if=\"item.status === 0\">\r\n                                            <!-- <a @click=\"viewDetails(item.id)\" class=\"mr-2\" data-toggle=\"modal\"\r\n                                                data-target=\"#exampleModal2\">\r\n                                                <i class=\"fa fa-eye\"\r\n                                                    style=\"color: rgb(244, 221, 18);border: 2px solid rgb(244, 221, 18);padding: 3px;font-size: 16px;cursor: pointer;\"></i>\r\n                                            </a> -->\r\n\r\n                                            <a @click=\"inactivestatus(item.id)\" class=\"mr-2\">\r\n                                                <i class=\"fa fa-times\"\r\n                                                    style=\"color:#ff0a0a;border: 2px solid #ff0a0a;padding: 3px;font-size: 16px;cursor: pointer;\"></i>\r\n                                            </a>\r\n\r\n                                            <a @click=\"activestatus(item.id)\" class=\"mr-2\">\r\n                                                <i class=\"fa fa-check\"\r\n                                                    style=\"color: #57b75e;border: 2px solid #57b75e;padding: 3px;font-size: 16px;cursor: pointer;\"></i>\r\n                                            </a>\r\n                                        </div>\r\n\r\n                                        <div v-else-if=\"item.status === 1\">\r\n                                            <!-- <a @click=\"viewDetails(item.id)\" class=\"mr-2\" data-toggle=\"modal\"\r\n                                                data-target=\"#exampleModal2\">\r\n                                                <i class=\"fa fa-eye\"\r\n                                                    style=\"color: rgb(244, 221, 18);border: 2px solid rgb(244, 221, 18);padding: 3px;font-size: 16px;cursor: pointer;\"></i>\r\n                                            </a> -->\r\n\r\n                                            <a @click=\"inactivestatus(item.id)\" class=\"mr-2\">\r\n                                                <i class=\"fa fa-times\"\r\n                                                    style=\"color:#ff0a0a;border: 2px solid #ff0a0a;padding: 3px;font-size: 16px;cursor: pointer;\"></i>\r\n                                            </a>\r\n                                        </div>\r\n\r\n                                        <div v-else-if=\"item.status === 2\">\r\n                                            <!-- <a @click=\"viewDetails(item.id)\" class=\"mr-2\" data-toggle=\"modal\"\r\n                                                data-target=\"#exampleModal2\">\r\n                                                <i class=\"fa fa-eye\"\r\n                                                    style=\"color: rgb(244, 221, 18);border: 2px solid rgb(244, 221, 18);padding: 3px;font-size: 16px;cursor: pointer;\"></i>\r\n                                            </a> -->\r\n\r\n                                            <a @click=\"activestatus(item.id)\" class=\"mr-2\">\r\n                                                <i class=\"fa fa-check\"\r\n                                                    style=\"color: #57b75e;border: 2px solid #57b75e;padding: 3px;font-size: 16px;cursor: pointer;\"></i>\r\n                                            </a>\r\n                                        </div>\r\n\r\n                                    </td>\r\n\r\n                                </tr>\r\n\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- Modal Start -->\r\n\r\n        <!-- Modal End -->\r\n\r\n\r\n\r\n\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\n\r\nimport \"jquery/dist/jquery.min.js\";\r\nimport \"bootstrap/dist/css/bootstrap.min.css\";\r\nimport \"datatables.net-dt/js/dataTables.dataTables\";\r\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\r\nimport $ from \"jquery\";\r\nimport { Form, Field } from 'vee-validate';\r\nimport * as Yup from \"yup\";\r\n\r\n// DataTable.use(DataTablesCore);\r\nexport default {\r\n    components: {\r\n        // DataTable\r\n        Form, Field\r\n    },\r\n\r\n    data() {\r\n        const storageData = JSON.parse(localStorage.getItem('user'));\r\n\r\n        const schema = Yup.object().shape({\r\n            designation_name: Yup.string().required(\"Designation Name is required\"),\r\n            status: Yup.string().required(\"Status is required\"),\r\n\r\n        });\r\n\r\n\r\n        return {\r\n            schema,\r\n            backend_url: process.env.VUE_APP_API_URL,\r\n            storageData,\r\n            user: \"\",\r\n            designations: \"\",\r\n            transferType: \"\",\r\n            office: \"\",\r\n            edit: false,\r\n            url: this.backendUrl + \"/api/users\",\r\n            basicInfo: {\r\n                name: '',\r\n                mobileNumber: '',\r\n                email: '',\r\n                designation: '',\r\n                designation_id: '',\r\n                image: '',\r\n            },\r\n            transferRecordById: [],\r\n            transferList: [],\r\n            designation: {\r\n                designation_name: \"\",\r\n                status: \"\"\r\n            }\r\n\r\n        };\r\n    },\r\n\r\n    computed: {},\r\n\r\n    methods: {\r\n        clear() {\r\n            this.designation.designation_name = \"\";\r\n            this.designation.status = \"\";\r\n        },\r\n        getTransferList() {\r\n            this.axios\r\n                .get(this.backend_url + 'getDesignationMgt?user_id=' + this.storageData.id)\r\n                .then((res) => {\r\n                    console.log(res.data.data);\r\n                    this.transferList = res.data.data;\r\n                    setTimeout(function () {\r\n                        $(\"#datatable\").DataTable({\r\n                            \"processing\": true,\r\n                        });\r\n                    }, 100);\r\n                })\r\n                .catch((e) => {\r\n                    console.log(e)\r\n                })\r\n        },\r\n        deletebutton(id) {\r\n            this.axios\r\n                .delete(this.backend_url + 'deleteTransferRecord/' + id)\r\n                .then((res) => {\r\n                    console.log('Delete Successfully', res);\r\n                    this.getTransferList();\r\n                    $('#datatable').DataTable().destroy();\r\n                    this.$toast.error(`Delete Successfully!!`);\r\n\r\n                })\r\n        },\r\n        viewDetails(id) {\r\n            console.log(id);\r\n            this.axios\r\n                .get(this.backend_url + 'specificUserTransferRecord/' + id)\r\n                .then((response) => {\r\n                    console.log(response);\r\n                    this.transferRecordById = response.data.data;\r\n                    console.log(this.transferRecordById.transfer_type);\r\n                    this.designation.id = id;\r\n                    this.designation.employee_name = this.transferRecordById.employee_name;\r\n                    this.designation.employee_id = this.transferRecordById.employee_id;\r\n                    this.designation.transfer_type = this.transferRecordById.transfer_type;\r\n                    this.designation.transfer_order = this.transferRecordById.transfer_order;\r\n                    this.designation.transfer_order_number = this.transferRecordById.transfer_order_number;\r\n                    this.designation.to_office = this.transferRecordById.to_office;\r\n                    this.designation.from_office = this.transferRecordById.from_office;\r\n                    this.designation.to_designation = this.transferRecordById.to_designation;\r\n                    this.designation.from_designation = this.transferRecordById.from_designation;\r\n                    this.designation.transfer_date = this.transferRecordById.transfer_date;\r\n                    this.designation.join_date = this.transferRecordById.join_date;\r\n                    this.designation.transfer_letter = this.transferRecordById.transfer_letter;\r\n                    this.designation.to_designation = this.transferRecordById.to_designation;\r\n                    this.designation.from_designation = this.transferRecordById.from_designation;\r\n\r\n                    this.edit = false;\r\n                })\r\n                .catch((e) => {\r\n                    console.log(e)\r\n                })\r\n        },\r\n        getdesignationList() {\r\n            this.axios\r\n                .get(this.backend_url + 'getDesignationMgt')\r\n                .then((res) => {\r\n                    // console.log(res.data.data);\r\n                    this.designations = res.data.data;\r\n\r\n                })\r\n        },\r\n        getTransfertype() {\r\n            this.axios\r\n                .get(this.backend_url + 'transferType')\r\n                .then((res) => {\r\n                    this.transferType = res.data.list;\r\n                    // console.log('fddsfsdf');\r\n                    // console.log(this.transferType[0].title);\r\n                })\r\n        },\r\n        getUser() {\r\n            this.axios\r\n                .get(this.backend_url + `users/${this.storageData.id}`)\r\n                .then((res) => {\r\n                    this.user = res.data.userDetail.employee_id;\r\n                    // console.log(this.user);\r\n                    // this.designation.employee_id = this.user\r\n                    // console.log(this.storageData.id);\r\n                })\r\n        },\r\n        getDesignationList() {\r\n            this.axios\r\n                .get(this.backend_url + 'getDesignationMgt')\r\n                .then((res) => {\r\n                    this.designations = res.data.data;\r\n                    // console.log(this.designations);\r\n                })\r\n        },\r\n        getOffice() {\r\n            this.axios\r\n                .get(this.backend_url + `getOfficeMgt`)\r\n                .then((response) => {\r\n                    this.office = response.data.list;\r\n                    // console.log(this.office);\r\n                })\r\n        },\r\n        activestatus(id) {\r\n            this.axios\r\n                .patch(this.backend_url + `activeDesignationMgtRecord/${id}`)\r\n                .then((response) => {\r\n                    console.log(response);\r\n                    this.getTransferList();\r\n                    $('#datatable').DataTable().destroy();\r\n                    this.$toast.success(`Active status Successfully!!`);\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n        inactivestatus(id) {\r\n            this.axios\r\n                .patch(this.backend_url + `inactiveDesignationMgtRecord/${id}`)\r\n                .then((response) => {\r\n                    console.log(response);\r\n                    this.getTransferList();\r\n                    $('#datatable').DataTable().destroy();\r\n                    this.$toast.error(`Status Cancel!!`);\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n        saveTranfer() {\r\n            try {\r\n                // this.designation.employee_id = this.storageData.id;\r\n                this.edit = false;\r\n                this.axios\r\n                    .post(this.backend_url + 'addDesignationMgt', this.designation)\r\n                    .then((res) => {\r\n                        console.log(res);\r\n                        this.getTransferList();\r\n                        $(\"#exampleModal\").modal('hide');\r\n                        $('#datatable').DataTable().destroy();\r\n                        this.$toast.success(`Add Data Successfully!`);\r\n                    })\r\n                    .catch((error) => {\r\n                        console.error(\"An error occurred while saving the transfer record:\", error);\r\n                        this.$toast.error(`Failed to save transfer record.`);\r\n                    });\r\n            } catch (error) {\r\n                console.error(\"An error occurred:\", error);\r\n                this.$toast.error(`Failed to save transfer record.`);\r\n            }\r\n        },\r\n    },\r\n\r\n    created() {\r\n        this.getTransferList();\r\n        this.getdesignationList();\r\n        this.getDesignationList();\r\n        this.getTransfertype();\r\n        this.getUser();\r\n        this.getOffice();\r\n    },\r\n\r\n    watch: {\r\n\r\n    },\r\n\r\n}\r\n\r\n\r\n</script>\r\n  \r\n<style>\r\n@import 'datatables.net-dt';\r\n\r\n.ui.inverted.dimmer {\r\n    color: #009688 !important;\r\n\r\n}\r\n\r\n.cropper {\r\n    height: 400px;\r\n    width: 400px;\r\n    background: #DDD;\r\n}\r\n\r\n\r\n.file-btn {\r\n    position: relative;\r\n    display: block;\r\n    cursor: pointer;\r\n}\r\n\r\n.file-btn input {\r\n    position: absolute;\r\n    height: 100%;\r\n    width: 100%;\r\n    left: 0;\r\n    top: 0;\r\n    opacity: 0;\r\n    cursor: pointer;\r\n}\r\n</style>\r\n  \r\n<style scoped>\r\n::v-deep .modal-container {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n::v-deep .modal-content {\r\n    position: relative;\r\n    display: flex;\r\n    flex-direction: column;\r\n    margin: 0 1rem;\r\n    padding: 1rem;\r\n    border: 1px solid #e2e8f0;\r\n    border-radius: 0.25rem;\r\n    background: #fff;\r\n}\r\n\r\n.modal__title {\r\n    margin: 0 2rem 0 0;\r\n    font-size: 1.5rem;\r\n    font-weight: 700;\r\n}\r\n\r\n.modal__close {\r\n    position: absolute;\r\n    top: 0.5rem;\r\n    right: 0.5rem;\r\n}\r\n</style>\r\n    \r\n<style scoped>\r\n.dark-mode div::v-deep .modal-content {\r\n    border-color: #2d3748;\r\n    background-color: #1a202c;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}