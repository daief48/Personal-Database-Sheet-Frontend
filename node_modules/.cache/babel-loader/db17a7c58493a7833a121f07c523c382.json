esignation: null,\n        transfer_date: null,\n        join_date: null,\n        transfer_letter: null\n      }\n    };\n  },\n\n  computed: {},\n  methods: {\n    editbutton(id) {\n      console.log(id);\n      this.axios.get(this.backend_url + 'specificUserTransferRecord/' + id).then(response => {\n        this.transferList = response.data;\n        console.log(this.transferList.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    getTransferList() {\n      this.axios.get(this.backend_url + 'getTransferList?user_id=' + this.storageData.id).then(response => {\n        this.transferList1 = response.data;\n        console.log(this.transferList1);\n        setTimeout(function () {\n          $(\"#datatable\").DataTable({\n            \"processing\": true\n          });\n        }, 100);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    getDepartmentList() {\n      this.axios.get(this.backend_url + 'getDepartment').then(res => {\n        // console.log(res.data.data);\n        this.departments = res.data.data;\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data; // console.log(this.designations);\n      });\n    },\n\n    getTransfertype() {\n      this.axios.get(this.backend_url + 'transferType').then(res => {\n        this.transferType = res.data.list; // console.log('fddsfsdf');\n        // console.log(this.transferType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id; // console.log(this.user);\n\n        this.tansfer.employee_id = this.user; // console.log(this.storageData.id);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list; // console.log(this.office);\n      });\n    },\n\n    saveTranfer() {\n      // this.tansfer.employee_id = this.storageData.id;\n      this.axios.post(this.backend_url + 'addTransferRecord', this.tansfer).then(res => {\n        console.log(res);\n        this.getTransferList();\n        $(\"#exampleModal\").modal('hide');\n      });\n    }\n\n  },\n\n  created() {\n    this.getTransferList();\n    this.getDepartmentList();\n    this.getDesignationList();\n    this.getTransfertype();\n    this.getUser();\n    this.getOffice();\n  },\n\n  mounted() {},\n\n  watch: {}\n};","map":{"version":3,"mappings":"AAuOA;AACA;AACA;AACA,OAAO,2BAAP;AACA,OAAO,sCAAP;AACA,OAAO,4CAAP;AACA,OAAO,iDAAP;AACA,OAAOA,CAAP,MAAc,QAAd,C,CACA;;AACA,eAAe;EACbC,UAAU,EAAE,CACV;EADU,CADC;;EAObC,IAAI,GAAG;IACL,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAX,CAApB,CADK,CAEL;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;;IACA,OAAO;MACLC,WAAW,EAAEC,OAAO,CAACC,GAAR,CAAYC,eADpB;MAELR,WAFK;MAGLS,IAAI,EAAE,IAHD;MAILC,WAAW,EAAE,IAJR;MAKLC,YAAY,EAAE,IALT;MAMLC,YAAY,EAAE,IANT;MAOLC,MAAM,EAAE,IAPH;MAQLC,GAAG,EAAE,KAAKC,UAAL,GAAkB,YARlB;MASLC,SAAS,EAAE;QACTC,IAAI,EAAE,EADG;QAETC,YAAY,EAAE,EAFL;QAGTC,KAAK,EAAE,EAHE;QAITC,WAAW,EAAE,EAJJ;QAKTC,cAAc,EAAE,EALP;QAMTC,KAAK,EAAE;MANE,CATN;MAiBL;MACAC,YAAY,EAAE,EAlBT;MAmBLC,aAAa,EAAE,EAnBV;MAoBLC,OAAO,EAAE;QACPC,WAAW,EAAE,IADN;QAEPC,aAAa,EAAE,IAFR;QAGPC,cAAc,EAAE,IAHT;QAIPC,qBAAqB,EAAE,IAJhB;QAKPC,SAAS,EAAE,IALJ;QAMPC,WAAW,EAAE,IANN;QAOPC,aAAa,EAAE,IAPR;QAQPC,eAAe,EAAE,IARV;QASPC,cAAc,EAAE,IATT;QAUPC,gBAAgB,EAAE,IAVX;QAWPC,aAAa,EAAE,IAXR;QAYPC,SAAS,EAAE,IAZJ;QAaPC,eAAe,EAAE;MAbV;IApBJ,CAAP;EAqCD,CAjEY;;EAmEbC,QAAQ,EAAE,EAnEG;EAqEbC,OAAO,EAAE;IAEPC,UAAU,CAACC,EAAD,EAAK;MACbC,OAAO,CAACC,GAAR,CAAYF,EAAZ;MACA,KAAKG,KAAL,CACGC,GADH,CACO,KAAKzC,WAAL,GAAmB,6BAAnB,GAAmDqC,EAD1D,EAEGK,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKzB,YAAL,GAAoByB,QAAQ,CAACjD,IAA7B;QACA4C,OAAO,CAACC,GAAR,CAAY,KAAKrB,YAAL,CAAkBxB,IAA9B;MACD,CALH,EAMGkD,KANH,CAMUC,CAAD,IAAO;