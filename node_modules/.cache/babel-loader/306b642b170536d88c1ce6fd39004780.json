{"ast":null,"code":"import \"jquery/dist/jquery.min.js\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"datatables.net-dt/js/dataTables.dataTables\";\nimport \"datatables.net-dt/css/jquery.dataTables.min.css\";\nimport $ from \"jquery\";\nimport { Form, Field } from 'vee-validate';\nimport * as Yup from \"yup\";\n\n// DataTable.use(DataTablesCore);\nexport default {\n  components: {\n    // DataTable\n    Form,\n    Field\n  },\n  data() {\n    const storageData = JSON.parse(localStorage.getItem('user'));\n    const schema = Yup.object().shape({\n      office_name: Yup.string().required(\"Office Name is required\"),\n      status: Yup.string().required(\"Status is required\")\n    });\n    const editschema = Yup.object().shape({\n      office_name: Yup.string().required(\"Office Name is required\"),\n      status: Yup.string().required(\"Status is required\")\n    });\n    return {\n      schema,\n      editschema,\n      backend_url: process.env.VUE_APP_API_URL,\n      storageData,\n      user: \"\",\n      offices: \"\",\n      designations: \"\",\n      OfficeType: \"\",\n      edit: false,\n      url: this.backendUrl + \"/api/users\",\n      basicInfo: {\n        name: '',\n        mobileNumber: '',\n        email: '',\n        designation: '',\n        designation_id: '',\n        image: ''\n      },\n      OfficeRecordById: [],\n      OfficeList: [],\n      editoffice: {\n        office_name: \"\",\n        status: \"\"\n      },\n      office: {\n        office_name: \"\",\n        status: \"\"\n      }\n    };\n  },\n  computed: {},\n  methods: {\n    onSubmit(values, {\n      resetForm\n    }) {\n      console.log(values); // send data to API\n      // reset the form and the field values to their initial values\n      resetForm();\n    },\n    deleterecord(id) {\n      this.axios.delete(this.backend_url + 'deleteOfficeMgt/' + id).then(res => {\n        console.log('Delete Successfully', res.message);\n        this.getOfficeList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(\"Delete Successfully\");\n      }).catch(error => {\n        console.error(\"An error occurred while updating the transfer record:\", error);\n        this.$toast.error(\"Record cannot be deleted. Associated data exists.\");\n      });\n    },\n    EditTranfer() {\n      try {\n        this.axios.put(this.backend_url + `updateOfficeMgt/${this.office.id}`, this.office).then(res => {\n          delete this.office;\n          console.log(this.office);\n          console.log(res);\n          this.getOfficeList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Update Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while updating the transfer record:\", error);\n          this.$toast.error(`Failed to update transfer record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to update transfer record.`);\n      }\n    },\n    editbutton(emp) {\n      // console.log(emp);\n      // office: {\n      //     office_name: \"\",\n      //     status: \"\"\n      // }\n      this.editoffice.id = emp.id;\n      this.editoffice.office_name = emp.office_name;\n      this.editoffice.status = emp.status;\n      console.log(this.editoffice);\n      this.edit = true;\n    },\n    clear() {\n      this.edit = false;\n      delete this.office;\n      console.log(this.office);\n    },\n    getOfficeList() {\n      this.axios.get(this.backend_url + 'getOfficeMgt?user_id=' + this.storageData.id).then(res => {\n        console.log(res.data);\n        this.OfficeList = res.data.list;\n        setTimeout(function () {\n          $(\"#datatable\").DataTable({\n            \"processing\": true\n          });\n        }, 100);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getofficeList() {\n      this.axios.get(this.backend_url + 'getoffice').then(res => {\n        // console.log(res.data.data);\n        this.offices = res.data.data;\n      });\n    },\n    getOfficetype() {\n      this.axios.get(this.backend_url + 'OfficeType').then(res => {\n        this.OfficeType = res.data.list;\n        // console.log('fddsfsdf');\n        // console.log(this.OfficeType[0].title);\n      });\n    },\n\n    getUser() {\n      this.axios.get(this.backend_url + `users/${this.storageData.id}`).then(res => {\n        this.user = res.data.userDetail.employee_id;\n        // console.log(this.user);\n        // this.office.employee_id = this.user\n        // console.log(this.storageData.id);\n      });\n    },\n\n    getDesignationList() {\n      this.axios.get(this.backend_url + 'getDesignationMgt').then(res => {\n        this.designations = res.data.data;\n        // console.log(this.designations);\n      });\n    },\n\n    getOffice() {\n      this.axios.get(this.backend_url + `getOfficeMgt`).then(response => {\n        this.office = response.data.list;\n        // console.log(this.office);\n      });\n    },\n\n    activestatus(id) {\n      this.axios.patch(this.backend_url + `activeOfficeMgtRecord/${id}`).then(response => {\n        console.log(response);\n        this.getOfficeList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.success(`Active status Successfully!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    inactivestatus(id) {\n      this.axios.patch(this.backend_url + `inactiveOfficeMgtRecord/${id}`).then(response => {\n        console.log(response);\n        this.getOfficeList();\n        $('#datatable').DataTable().destroy();\n        this.$toast.error(`Status Cancel!!`);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    saveTranfer() {\n      try {\n        // this.office.employee_id = this.storageData.id;\n        this.edit = false;\n        this.axios.post(this.backend_url + 'addOfficeMgt', this.office).then(res => {\n          console.log(res);\n          this.getOfficeList();\n          $(\"#exampleModal\").modal('hide');\n          $('#datatable').DataTable().destroy();\n          this.$toast.success(`Add Data Successfully!`);\n        }).catch(error => {\n          console.error(\"An error occurred while saving the Office record:\", error);\n          this.$toast.error(`Failed to save Office record.`);\n        });\n      } catch (error) {\n        console.error(\"An error occurred:\", error);\n        this.$toast.error(`Failed to save Office record.`);\n      }\n    }\n  },\n  created() {\n    this.getOfficeList();\n    this.getofficeList();\n    this.getDesignationList();\n    this.getOfficetype();\n    this.getUser();\n    // this.getOffice();\n  },\n\n  watch: {}\n};","map":{"version":3,"mappings":"AAkTA,OAAO,2BAA2B;AAClC,OAAO,sCAAsC;AAC7C,OAAO,4CAA4C;AACnD,OAAO,iDAAiD;AACxD,OAAOA,OAAO,QAAQ;AACtB,SAASC,IAAI,EAAEC,KAAI,QAAS,cAAc;AAC1C,OAAO,KAAKC,GAAE,MAAO,KAAK;;AAE1B;AACA,eAAe;EACXC,UAAU,EAAE;IACR;IACAH,IAAI;IAAEC;EACV,CAAC;EAEDG,IAAIA,GAAG;IACH,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;IAE5D,MAAMC,MAAK,GAAIR,GAAG,CAACS,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC;MAC9BC,WAAW,EAAEX,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,yBAAyB,CAAC;MAC7DC,MAAM,EAAEd,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,oBAAoB;IACtD,CAAC,CAAC;IAEF,MAAME,UAAS,GAAIf,GAAG,CAACS,MAAM,CAAC,CAAC,CAACC,KAAK,CAAC;MAClCC,WAAW,EAAEX,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,yBAAyB,CAAC;MAC7DC,MAAM,EAAEd,GAAG,CAACY,MAAM,CAAC,CAAC,CAACC,QAAQ,CAAC,oBAAoB;IACtD,CAAC,CAAC;IACF,OAAO;MACHL,MAAM;MACNO,UAAU;MACVC,WAAW,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAe;MACxChB,WAAW;MACXiB,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,EAAE;MACXC,YAAY,EAAE,EAAE;MAChBC,UAAU,EAAE,EAAE;MACdC,IAAI,EAAE,KAAK;MACXC,GAAG,EAAE,IAAI,CAACC,UAAS,GAAI,YAAY;MACnCC,SAAS,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,cAAc,EAAE,EAAE;QAClBC,KAAK,EAAE;MACX,C